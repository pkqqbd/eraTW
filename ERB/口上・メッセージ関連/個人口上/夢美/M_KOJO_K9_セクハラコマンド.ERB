@M_KOJO_MESSAGE_COM_K9_330_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
		;時間停止時
		PRINTFORMDW 在停止的时间里对梦美的%SHOW_BOTTOM(TARGET,1)%下手。
		PRINTFORMDW 当然，没有抵抗，很容易就看到了其中的内容。
		PRINTFORMDW 但是另一方面，因为时间静止，没有反应，也有点空虚。
	ELSE
		SELECTCASE TFLAG:193
			CASE 1
				;めくれますが夢美ちゃん、是てます？
				IF CFLAG:291
					;沒穿よ
					IF ABL:14 >= 8
						;露出癖Lv8以上の夢美ちゃんは没穿内裤でお出かけも出来ちゃうえっちな大学教授です
						IF PALAM:9 >= 5000
							;びちょびちょじゃないかよオイ
							CALL HPH_PRINT,@"「見て%CALLNAME:MASTER%、私のアソコ、今こんなになっちゃってるの……HPH」","W"
							PRINTFORMDW 開心的梦美一边自己捋起%SHOW_BOTTOM(TARGET,1)%、把阴毛茂盛的秘部给展示出来
							PRINTFORMDW 衣服から解き被释放进雌の匂いが%CALLNAME:MASTER%の鼻腔を刺激してやまない
							PRINTFORML 「呐……不仅仅是在看……吧?」
							PRINTFORMDW 浮现出淫靡笑容的梦美在%CALLNAME:MASTER%的耳边低语
							PRINTFORMW 
							CALL HPH_PRINT,@"「――シ・てHPH」","W"
						ELSE
							CALL HPH_PRINT,@"「诶嘿嘿……ねぇ見て%CALLNAME:MASTER%、私今日、沒穿のよ……HPH」","W"
							PRINTFORMDW 带着淫笑的梦美一边自己捋起%SHOW_BOTTOM(TARGET,1)%、把阴毛茂盛的秘部给展示出来
							PRINTFORMDW 眼前的景色令%CALLNAME:MASTER%窒息
							CALL HPH_PRINT,@"「やだ……HPH　%CALLNAME:MASTER%に見られたら……濡れてきちゃったHPH」","W"
							PRINTFORMDW 正如梦美所说，爱液已经从阴道口渗出，带着淫靡的热气。
							PALAM:9 += 500
						ENDIF
					ELSEIF ABL:14 < 8 && ABL:14 >= 6
						;露出癖Lv6以上、どこでもまくってくれる
						IF PALAM:9 >= 5000
							;びちょびちょじゃないか
							CALL HPH_PRINT,@"「ふふ……私のびちゃびちゃに濡れてるスケベまんこ、見たいの……？HPH」","W"
							CALL HPH_PRINT,@"「いいわよ、見せたげる……HPH　んふ、どうかしら……？」","W"
							PRINTFORMDW 一边这么说梦美一边自己捋起%SHOW_BOTTOM(TARGET,1)%、把阴毛茂盛的秘部给展示出来
							PRINTFORMDW %CALLNAME:MASTER%的鼻腔被解開衣服释放出的雌性气息刺激……
						ELSE
							CALL HPH_PRINT,@"「稍微……%CALLNAME:MASTER%、今日、沒穿のに……HPH」","W"
							PRINTFORMDW 一边这么说梦美一边自己捋起%SHOW_BOTTOM(TARGET,1)%，把阴毛茂盛的秘部给展示出来
							CALL HPH_PRINT,@"「ほんと%CALLNAME:MASTER%ったら、物好きよね……HPH」","W"
						ENDIF
					ELSEIF ABL:14 < 6 && ABL:14 >= 3
						;露出癖Lv3以上6未満
						IF GET_TARGETNUM() >= 2
							;他に誰か居る
							PRINTFORML 「稍微%CALLNAME:MASTER%、ダメよここじゃ……」
							PRINTFORML 「人が……居ないところでなら、その……」
							PRINTFORML 「見せて、あげる……から……ね？」
						ELSE
							PRINTFORML 「已经那么想看了吗?%CALLNAME:MASTER%也真喜欢啊」
							PRINTFORML 「你看……今天……那个……」
							PRINTFORML 「没有穿内裤……是的……不要那么认真看……\/\/\/\/」
						ENDIF
					ELSE
						;露出癖Lv3未満
						PRINTFORML 「等等，住手，住手……是的，现在是那个……！」
						PRINTFORML （没穿ーーーー啊！！\/\/\/\/\/）
					ENDIF
				;是てるよ
				ELSE
					IF ABL:14 >= 6
						;露出癖Lv6以上、どこでもまくってくれる
						CALL HPH_PRINT,@"「ふふ……見たい……？いいわよ……HPH」","W"
						PRINTFORMDW 这么说着，梦美毫不犹豫地掀了起来,露出了内衣%SHOW_BOTTOM(TARGET,1)%
						CALL HPH_PRINT,@"「今日は%PANTSNAME(EQUIP:TARGET:下半身内衣２, TARGET)%だけど、どうかしらHPH」","W"
						PRINTFORML 「喂，看着就好了…………？」
					ELSEIF ABL:14 < 3
						;露出癖Lv3未満
						PRINTFORML 「不行，不行的话!那样做的话，就会看到羞耻的样子……\/\/\/」
					ELSE
						;露出癖Lv3以上6未満
						IF GET_TARGETNUM() >= 2
							;人が居る
							PRINTFORML 「可是、这里确实不行啊……别人看见了怎么办?……\/\/\/」
							PRINTFORML 「如果是没有人的地方，无论多少都可以看……啊?」
							PRINTFORML 「啊，等等，所以，你不是说因为看不见所以不行吗?……\/\/\/」
							PRINTFORMDW 見たいからめくってるんじゃないか。
							PRINTFORML 「も、もう！ばか……\/\/\/」
						ELSE
							;人が居ない
							PRINTFORML 「已经那么想看了吗?%CALLNAME:MASTER%也真喜欢啊」
							PRINTFORMDW 梦美害羞地掀了掀，内衣露了出来，%SHOW_BOTTOM(TARGET,1)%
							PRINTFORML 「你看.....今天……%PANTSNAME(EQUIP:TARGET:下半身内衣２, TARGET)%怎么样啊……かしら？\/\/\/」
						ENDIF
					ENDIF
				ENDIF
			CASE 0
				;やだもー(嬉しげ)
				PRINTFORML 「已经、%CALLNAME:MASTER%啊、算了……\/\/\/\/」
			CASE -1
				;やだもー(嫌)
				PRINTFORML 「稍微、请停止像傻瓜一样的模仿行为」
			CASE -3
				;つねるぞ痴漢痴女たち
				PRINTFORML 「突然伸手去摸哪里啊，变态」
				PRINTFORMDW 原来如此!请别扯皮!
				PRINTFORML 「哈？只是没有被打罢了」
			CASE -5
				;最大の憤怒と嫌悪を込めたボカッ！ボカッ！
				PRINTFORML 「什！？突然要干什么啊，变态！！」
				PRINTFORMDW angry！angry！
				PRINTFORMDW 不止是被骂，还被梦美狠狠地揍了一顿!好像要从身上冒热气!
				PRINTFORML 「这种程度就过去了，你要心存感激」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
