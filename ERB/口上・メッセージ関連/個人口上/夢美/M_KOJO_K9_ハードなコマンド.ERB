;==================================================
;144,放尿
;==================================================
@M_KOJO_MESSAGE_COM_K9_144
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K9_144_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K9_144_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「诶! ?诶，诶诶诶诶诶……这……你说，想看! ?」
		PRINTFORML 「诶，你到底是怎么想的」
		PRINTFORMDW 明显地失望了，梦美開始动摇了。
		PRINTFORML 「对，不用那么失望……因为，那是排泄行为啊?看，不是给人看的东西啦!」
		PRINTFORMDW 真的想看啊，想看啊。能给我看看吗?
		PRINTFORML 「啊，我已经知道了!我会给你看的!所以不要那么难过啊!！」
		PRINTFORML 「你，已经配得上变态这个称呼了呢」
		;洋式廁所かそうでないかでMSG分岐するよ
		; WC(14, 40, 236, 316, 322, 406, 418, 824, 906, 917)
		;洋式そうなところ(40,316,322,406,418,824,906,917)
		IF CFLAG:MASTER == 40 || CFLAG:MASTER == 316 || CFLAG:MASTER == 322 || CFLAG:MASTER == 406 || CFLAG:MASTER == 418 || CFLAG:MASTER == 824 || CFLAG:MASTER == 906 || CFLAG:MASTER == 917
			PRINTFORMDW 梦美准备好小便后，在马桶上坐下。
			PRINTFORMDW 面对面的状态下，在%CALLNAME:MASTER%的眼睛里，深深地烙印下了梦美的胯股之间。
		ELSE
			PRINTFORMDW 梦美蹲下来開始准备小便……
			PRINTFORMDW 从后面眺望的话，就会看到微微晃动的菊穴，如果从前面眺望的话，梦美的两股之间，就会映入%CALLNAME:MASTER%的眼帘……
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:恋慕
		PRINTFORML 「そ、それじゃあ、出すわね……\/\/\/\/」
		PRINTFORMDW 梦美可爱的脸颊一边染得通红一边開始小便。
		PRINTFORML 「んんっ……被%CALLNAME:MASTER%看着、看啊、留出来了哦……\/\/\/」
		PRINTFORMDW 羞赧的梦美的尿道口一下子冒出液体来。
		CALL HPH_PRINT,@"「ふぁぁ……HPH」","W"

		;廁所なら水に当たってじょぼじょぼ音が出るからね
		IF CFLAG:MASTER == 14 || CFLAG:MASTER == 40 || CFLAG:MASTER == 236 || CFLAG:MASTER == 316 || CFLAG:MASTER == 322 || CFLAG:MASTER == 406 || CFLAG:MASTER == 418 || CFLAG:MASTER == 824 || CFLAG:MASTER == 906 || CFLAG:MASTER == 917
			PRINTFORMDW 便器の中に溜まった水におしっこが当たり「ちょぼぼぼぼぼぼ……」という音が響く。
			PRINTFORMDW 排尿が進むにつれて勢いは衰え、それと共に「ちょろろろろ……ぴちゃ、ぴちゃん……」と響く音も変わっていく。
		ELSE
			PRINTFORMDW 释放出液体、一边流出液体一边制造出吱吱的声音
		ENDIF

		PRINTFORML 「啊......别这么仔细看着啊......\/\/\/」
		PRINTFORMDW 夢美は恥ずかしがってそう言ったが、%CALLNAME:MASTER%は構わず夢美の排尿姿を凝視し続けた。
		CALL HPH_PRINT,@"やがて排尿が終わると、夢美は「はぁっ……HPH」と熱い吐息を吐いて、ぶるりと震えた。","W"
		PRINTFORMDW 仔细一看，阴毛上也有飞溅的痕迹，稍微濡湿了一点
		CALL HPH_PRINT,@"「んんっ……はぁ……HPH　おしっこ、終わったわよ……満足……？」","W"

		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORML 「那……那个，看吧……\/\/\/」
		PRINTFORML 「んん……んっ……」
		PRINTFORMDW 夢从梦美的尿道口一下子流出一泡液体来

		;廁所なら水に当たってじょぼじょぼ音が出るからね
		IF CFLAG:MASTER == 14 || CFLAG:MASTER == 40 || CFLAG:MASTER == 236 || CFLAG:MASTER == 316 || CFLAG:MASTER == 322 || CFLAG:MASTER == 406 || CFLAG:MASTER == 418 || CFLAG:MASTER == 824 || CFLAG:MASTER == 906 || CFLAG:MASTER == 917
			PRINTFORMDW 便器の中に溜まった水におしっこが当たり「ちょぼぼぼぼぼぼ……」という音が響く。
			PRINTFORMDW 排尿が進むにつれて勢いは衰え、それと共に「ちょろろろろ……ぴちゃ、ぴちゃん……」と響く音も変わっていく。
		ELSE
			PRINTFORMDW 释放出液体、一边流出液体一边制造出吱吱的声音
		ENDIF

		PRINTFORML 「噢……被人看着，尿尿时被人看着……\/\/\/」
		PRINTFORMDW 不久排尿结束，梦美全身颤抖起来。
		PRINTFORMDW 仔细一看，阴毛上也有飞溅的痕迹，稍微濡湿了一点
		PRINTFORML 「んっ……ふぅ……\/\/\/」

		RETURN 1
	;それ以外
	ELSE
		PRINTFORML 「……んっ」
		PRINTFORMDW 夢从梦美的尿道口一下子流出一泡液体来

		;廁所なら水に当たってじょぼじょぼ音が出るからね
		IF CFLAG:MASTER == 14 || CFLAG:MASTER == 40 || CFLAG:MASTER == 236 || CFLAG:MASTER == 316 || CFLAG:MASTER == 322 || CFLAG:MASTER == 406 || CFLAG:MASTER == 418 || CFLAG:MASTER == 824 || CFLAG:MASTER == 906 || CFLAG:MASTER == 917
			PRINTFORMDW 便器の中に溜まった水におしっこが当たり「ちょぼぼぼぼぼぼ……」という音が響く。
			PRINTFORMDW 排尿が進むにつれて勢いは衰え、それと共に「ちょろろろろ……ぴちゃ、ぴちゃん……」と響く音も変わっていく。
		ELSE
			PRINTFORMDW 释放出液体、一边流出液体一边制造出吱吱的声音
		ENDIF

		PRINTFORMW 「不要那样目不转睛地看啊…」
		PRINTFORMDW 不久排尿结束，梦美全身颤抖起来。
		PRINTFORMDW 仔细一看，阴毛上也有飞溅的痕迹，稍微濡湿了一点
		PRINTFORML 「んんっ……ふぅ」

		RETURN 1
	ENDIF

	;この見せたがりめ！
	IF ABL:14 > 5
		PRINTFORML 「ねえ%CALLNAME:MASTER%……見られながらおしっこするの、癖になっちゃいそうなんだけど……\/\/\/」
		PRINTFORMDW 「変態」と%CALLNAME:MASTER%が言うと夢美は満更でもない表情を浮かべてぶるりと震えた。
		PRINTFORML 「不要说出来啊……\/\/\/」
	ENDIF

ENDIF
RETURN 1

