;1,舐陰
;==================================================
@M_KOJO_MESSAGE_COM_K9_1
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K9_1_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K9_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「えっ……？わ、わたしの……その、あ、あそこを、な、舐める……の？」
		PRINTFORMDW 首を縦に振る%CALLNAME:MASTER%
		PRINTFORML 「ちょ、稍微待って！まだ心の準備が――ふぁぁああっ！？」
		PRINTFORMDW しどろもどろになる夢美を無視して、%CALLNAME:MASTER%は夢美の秘所に吸い付いた。
		CALL HPH_PRINT,@"「んぁっ、ちょ、ま、待ってってぇっHPH　い、言ったのにいっ」","W"
		PRINTFORMDW 夢美の陰毛の感触を顔面で感じながら%CALLNAME:MASTER%は秘豆を舌で擦り、秘唇を舐め解す。
		PRINTFORMDW そのうち、夢美の抵抗はなくなり、声にも鼻にかかった甜声色が混ざり始めた。
		CALL HPH_PRINT,@"「あぁんっ、あっ、ああっ……HPHいやっ、なん、なのぉ……この……感じ……っ」","W"
		PRINTFORML （もしかして……これが、カイカンってやつ、なの……？）
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;膣内射精直後
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:100 == PLAYER
		IF TCVAR:102
			IF TALENT:3 || TALENT:184
				CALL HPH_PRINT,@"「ああっ……HPHいま、おまんこ舐められたら……はあっ、%CALLNAME:MASTER%のが出てきちゃうよ……」","W"
				CALL HPH_PRINT,@"「もったいないわ……それとも、んぁっ、%CALLNAME:MASTER%も自分のせーえき、んぅっ、味わって、みたい、の……？HPH」","W"
				CALL HPH_PRINT,@"「あぁきもちぃっ……それならっ、いっぱいなめていいわ、よぉっ、そう、そこぉっ、あひっHPH」","W"
			ELSE
				PRINTFORML 「んんっ、あっ、やっ、いまそこ舐めたらっ……\/\/\/」
				PRINTFORML 「ナカからぁっ、出てっ、きちゃう、わ、よぉっ……あはあっ\/\/\/」
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;絶頂した
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:0
		IF ABL:0 > 5
			IF TALENT:73 == 1
				CALL HPH_PRINT,@"「あはぁっHPHいいっHPHそろそろ、イけそうなのぉっHPH","W"
				CALL HPH_PRINT,@"「そうよぉっ、もっとっ、もっとなめなめしてぇっHPHあっあっ、あ、ぁ、あ……」","W"
				CALL HPH_PRINT,@"「イくっ、いく、いく、ぁ、イクイク、あ、ぁーーーーーっHPH」","W"
			ELSE
				CALL HPH_PRINT,@"「はぁっ、きもちぃっHPH、%CALLNAME:MASTER%ぁっHPH」","W"
				CALL HPH_PRINT,@"「イクのぉっ、わたしっ、そろそろっHPH」","W"
				CALL HPH_PRINT,@"「だからっ、はぁぁっ、もっとぉ、ぺろぺろしてぇっHPH」","W"
			ENDIF
		ELSE
			PRINTFORML 「あっ、あっ、いやっ、いやぁっ、それ以上舐めないでぇっ！いっちゃう、いっちゃうからぁ！」
			PRINTFORML 「だめっ、だめ、いくっ、あっイく、あっ、あ、ぁ、イクっ、いやっ、イくぅっ！」
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMDW 止まった時間の中で、%CALLNAME:MASTER%は夢美の秘所を口に含んだ。
		PRINTFORMDW ふさふさとした陰毛の感触が顔に心地よいが、当の夢美は愛液を溢れさせつつも、微動だにしない。
		PRINTFORMDW %CALLNAME:MASTER%は時間停止を解除したときの反応を楽しみに、夢美の秘所を舐め続けた。
		RETURN 1
	ELSE
		SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「あひっ、ああっ、そんなっ……舐める音出さないでぇっ……\/\/\/」
				PRINTFORMDW 言葉とは裏腹に多くの愛液を溢れさせる夢美の秘所を見て、%CALLNAME:MASTER%は一層音を立てて舐めてやった。
				CALL HPH_PRINT,@"「ひっあっ、だから、おと、だめだってばぁっHPH」","W"
			CASE 1
				PRINTFORMDW %CALLNAME:MASTER%はわざとじゅるじゅると音を立てて夢美の愛液を啜った。
				PRINTFORML 「ふあああっ、あぁっ、きもちっ、けどぉ……そんなに音させないでぇ……っ\/\/\/」
				PRINTFORML （恥ずかしいのに……わたしのおまんこ、もっと疼いちゃってる……）
			CASE 2
				PRINTFORMDW %CALLNAME:MASTER%はくぱぁ、と夢美の秘所を割り開くと、中へ舌を侵入させる。
				PRINTFORML 「やぁぁっ、あはぁっ！そんなにっ、開いちゃやぁっ……\/\/\/」
				PRINTFORMDW 秘所の中に侵入した%CALLNAME:MASTER%の舌へ、夢美の膣肉が絡みつく。
				PRINTFORMDW それに対して%CALLNAME:MASTER%も舌で擦り返してやると、夢美がびくりと身体を震わせる。
				CALL HPH_PRINT,@"「ああぁぁあっ！そこっ、イイっ！そんなにっ、あぁ、こすらないでぇっHPH」","W"
			CASE 3
				PRINTFORMDW %CALLNAME:MASTER%はあえて秘所を舐めずに、夢美の陰毛を口に含んでみた。
				PRINTFORML 「ちょっ、なんでぇっ……そんなところ舐めるのよぉ……」
				PRINTFORMDW 夢美は秘所を舐めて貰えると期待していたところにお預けを食らい、腰をくねらせ始める。
				PRINTFORMDW ひとしきり彼女の陰毛を味わった%CALLNAME:MASTER%は、期待に汁を垂らす秘所へむしゃぶりついた。
				CALL HPH_PRINT,@"「あっ？！あぁーーーーーっHPH」","W"
			CASEELSE
				PRINTFORML 「ああっ、だめぇっ、クリっ、そんなにっ、舐めちゃやぁっ……\/\/\/」
				PRINTFORMDW 言葉では拒絶するが、秘豆を刺激され、びくびくと震える身体は愛液を一層溢れさせた。
				CALL HPH_PRINT,@"「ひっああああっHPHか、かわ、むいちゃやぁっ……！なかみ、びんかん、だから、ぁっ……」","W"
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1
