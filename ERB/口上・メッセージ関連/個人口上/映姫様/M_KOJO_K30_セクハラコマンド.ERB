	IF FLAG:70
		CALL K30_PRINT_BUTT_IMAGE(SELECTCOM)
	ELSEIF CFLAG:诶嘿嘿 == 2
		IF TEQUIP:TARGET:50 == MASTER
			IF RAND:2 == 1
				PRINTFORMW 「嗯~♥　能清楚、看见吧……」
				PRINTFORMW 「我的，这里…好好地吞下了你的…♥」
			ELSE
				PRINTFORMW 「哈~、哈啊…♥　%K30_C_NAME(MASTER)%，再多看看…」
				PRINTFORMW 「好舒服的地方♥ 进来了，看♥」
			ENDIF
		ELSEIF TEQUIP:TARGET:51 == MASTER
			IF RAND:2 == 1
				CALL HPH_PRINT, @"「呼呼呼っHPH　连接的地方，隐藏着……很在意？」", "L"
				CALL HPH_PRINT, @"「怎么样，用我的屁股…把你的，吞得这么下流HPH」", "W"
			ELSE
				CALL HPH_PRINT, @"「怎么样，看得清楚吗…HPH」", "L"
				CALL HPH_PRINT, @"「屁股…就这样展開…连下流的声音都发出来了HPH」", "W"
			ENDIF
		ELSEIF !EQUIP:下半身内衣２
			IF PALAM:潤滑 < PALAMLV:7
				PRINTFORMW 「怎么样？兴奋起来了吗……？」
			ELSE
				PRINTFORMW 「能看清吧……我的,这里」
				PRINTFORMW 「这样……粘糊糊的……太下流了…♥」
			ENDIF
		ELSE
			IF RAND:2 == 1
				PRINTFORMW 「我的%PANTSNAME(EQUIP:下半身内衣２, TARGET)%,现在正被盯着…」
				PRINTFORMW 「想脱，是吗？」
			;関係ないけど、随机でたまにとんでもないやつ是てるよね
			ELSE
				PRINTFORMW 「这个%PANTSNAME(EQUIP:下半身内衣２, TARGET)%…」
				PRINTFORMW 「适合我吗…♥」
			ENDIF
		ENDIF
		RETURN 1
	ELSE
		SELECTCASE TFLAG:193
			;ビンタ
			CASE -5
				PRINTDATAL
					DATAFORM 「……这样头脑稍微冷静一点了吗」
					DATAFORM 「……你是在開玩笑吗！？」
					DATAFORM 「……你到底在想什么？」
				ENDDATA
				PRINTDATAW
					DATAFORM 「你要知道羞耻！羞耻！」
					DATAFORM 「ああ…想说的话已经多得像山一样了」
					DATAFORM 「居然……对阎魔做了这样的事呢」
				ENDDATA
				CALL K30_LECTURE_CHECK(30 + RAND:10, 1)
			;つねられる
			CASE -3
				PRINTDATAW
					DATAFORM 「……是小孩吗，你」
					DATAFORM 「……擅自乱动的手不好？」\n「还是……手主人的脑子不好呢？」
					DATAFORM 「偷盗的手会被砍掉」\n「据说也有那样的惩罚」\n「对这种没有自制效果的手…什么样的惩罚才合适呢?？」
				ENDDATA
				CALL K30_LECTURE_CHECK(25 + RAND:15, 1)
			;嫌がる
			CASE -1
				PRINTDATAW
					DATAFORM 「被欲望驱使而活，一定会后悔的。」
					DATAFORM 「积累的善行……失去也只是一瞬间」
					DATAFORM 「主动去做别人讨厌的事情，这不是搞错了吗？」
					DATAFORM 「如果无故被这样做的话，谁都会讨厌的」
				ENDDATA
				CALL K30_LECTURE_CHECK(25 + RAND:10, 1)
			;呆れられる
			CASE 0
				PRINTDATAW
					DATAFORM 「ああ、已经…是这样了」\n「你这个人…是这样的人…」
					DATAFORM 「活得像自己是很棒的，但是」\n「在欲望的驱使下生活是怎样的呢?」
					DATAFORM 「%K30_C_NAME(MASTER)%我想也有很多好的地方」\n「光这一点就抵消了很多…」
				ENDDATA
			;成功（裤子用
			;人前で主动裤子下ろすのもたいがいだと思う
			CASE 1
				IF K30_BE_SEEN() == 0
					IF ABL:露出癖 > 3
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMW 「虽然很不好意思……但如果是你的话……」
							CASEELSE
								PRINTFORMW 「只想给你看……只想给%K30_C_NAME(MASTER)%你一人看啊…」
						ENDSELECT
						TFLAG:193 += 10
					ELSE
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMW 「就算只有我们两人……也还是很害羞…」
							CASEELSE
								PRINTFORMW 「ぁ……！　唔，唔，好害羞……っ」
						ENDSELECT
					ENDIF
				ELSE
					IF ABL:露出癖 > 5
						;ぱんつ是てる
						IF TEQUIP:0 > 1
							PRINTFORMW 「%K30_C_NAME(MASTER)%的请求啊…即使是这样，如果是你的话…♥」
						;没穿内裤でも平気？でたくし上げ
						ELSE
							IF RAND:2 == 1
								PRINTFORMW 「%K30_C_NAME(MASTER)%如果要看的话，我什么都可以…♥」
							ELSE
								PRINTFORMW 「我，被看到了……被看到这般的样子……我……♥」
							ENDIF
						ENDIF
					ELSE
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMW 「因为关系…阎魔的威严…就到这里吧…」
							CASEELSE
								PRINTFORMW 「……不行哦……%K30_C_NAME(K30_FIND_AROUND(),2)%…在看着呢…」
						ENDSELECT
					ENDIF
				ENDIF
			;成功（裙子用
			CASE 2
				PRINTFORMDL 映姬慢慢地回头看这边…%CALLNAME:MASTER%的头对%TEXTR("贴在/咚咚的")%拍打
				PRINTFORMDL 由于惊讶和害羞，脸颊都红了…
				PRINTFORMW 
				IF K30_BE_SEEN()
					PRINTDATAW
						DATAFORM 「那个…至少，谁都不在的地方…」\n「%K30_C_NAME(K30_FIND_AROUND(), 2)%、不是在吗…」
						DATAFORM 「这样的事…能不能在人前控制一下…？」\n「对我来说尊严和体面很重要…」
						DATAFORM 「退一百步说，如果只是给你看的话还可以」\n「%K30_C_NAME(K30_FIND_AROUND(), 1)%在那里就稍微…」
					ENDDATA
				ELSE
					PRINTDATAW
						DATAFORM 「啊啊、已经…」\n「对你做的事，一个一个地盯着看也没办法」\n「虽然明白是的…」
						DATAFORM 「真是的…%K30_C_NAME(MASTER)%又不是小孩子」\n「你冷静一点吧…」
						DATAFORM 「即使是给对方看也可以」\n「这种事需要心理准备，或者说是觉悟。」\n「…知道？」
					ENDDATA
				ENDIF
			;振動棒约会
			;约会中は裙子めくれないのでOMANEKIBEYA専用
			CASE 3
				PRINTFORMDL 在不断的快感中淋漓淫汁的同时，映姬无意识地摇动着屁股
				PRINTFORMDL %CALLNAME:MASTER%的手似乎在等待着玩弄…
				PRINTFORMW 
				STRDATA LOCALS
					DATAFORM 「%K30_C_NAME(MASTER)%…快点，来，这里…请HPH」\n「现在马上就可以了…HPH」
					DATAFORM 「只是在看…？」\n「屁股…就想要，我已经…忍耐了…HPH」
					DATAFORM 「%K30_C_NAME(MASTER)%…在这里哦，这里HPH」\n「快点，把这个拔出来…把你的…HPH」
				ENDDATA
				CALL HPH_PRINT, LOCALS, "L"
		ENDSELECT
		;日記のフラグ
		SIF DIARY:30:15 == 0
			CALL K30_WRITE_DIARY(15)
	ENDIF
ENDIF
RETURN 1
