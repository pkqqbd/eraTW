RETURN RESULT

@M_KOJO_MESSAGE_COM_K99_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:時間停止
		PRINTFORMW 
		RETURN 1
	;通常
	ELSE
		;恋慕
		IF TALENT:恋慕
			;この指令で你が射精
			IF NOWEX:MASTER:射精
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「唔呼呼……HPH 已经可以了哦……？HPH」","L"
						CALL HPH_PRINT,@"「没关系的……不用忍耐……HPH」","L"
						CALL HPH_PRINT,@"「我会把你的东西……全部收下的……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 丰姬撸动肉棒的速度渐渐快了起来
							PRINTFORMDL %CALLNAME:MASTER%根本无法忍受这样的快感
							PRINTFORMDW 把精液射在张開嘴等待的丰姬脸上……
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「嗯……我觉得有点浪费啊……HPH」","L"
						CALL HPH_PRINT,@"「不过、你坚持的话……HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDW 最后还是希望射在阴道里、这么想着的丰姬有些不太满意的样子……
					CASE 2
						SIF !CFLAG:99:1998
							PRINTFORMDL 丰姬娇柔的手淫、让%CALLNAME:MASTER%瞬间就射了出来
						CALL HPH_PRINT,@"「哇……HPH 这个量……HPH」","L"
						CALL HPH_PRINT,@"「还有、这气味……啊……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 丰姬把脸凑近粘在手掌上的精液
							PRINTFORMDW 深呼吸到极限、把浓重的污秽气味收进肺里……
						ENDIF
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「%CALLNAME:MASTER%的肉棒、像这样摸起来……HPH」","L"
						CALL HPH_PRINT,@"「我也有些……兴奋了啊……HPH」","L"
						CALL HPH_PRINT,@"「啊……不行……没办法忍受了啊……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 丰姬一只手搓着肉棒、空着一只手安慰自己
							PRINTFORMDW 那光景淫荡得光是看着就会射精……
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「污秽堆积在这里吗……？HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDL 丰姬一边轻柔地揉搓着阴囊、一边有规律地撸动着肉棒
						CALL HPH_PRINT,@"「有趣……竟然缩起来了……HPH」","L"
						CALL HPH_PRINT,@"「我会让你舒服起来的……HPH」","W"
					CASE 2
						CALL HPH_PRINT,@"「哦？你听见了吗……？HPH」","L"
						CALL HPH_PRINT,@"「有一种轻微的、色情的声音……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 沾满先走液的手指缠在龟头上、温柔地上下捋着
							PRINTFORMDW 直接的快乐配合视觉和声音、让%CALLNAME:MASTER%变得难以忍耐起来……
						ENDIF
				ENDSELECT
				RETURN 1
			ENDIF
		;それ以外
		ELSE
			;この指令で你が射精
			IF NOWEX:MASTER:射精
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「已经够了吗……？」
						PRINTFORML 「好啊……可以射精哦……♪」
						SIF !CFLAG:99:1998
							PRINTFORMDW 丰姬重点揉搓%CALLNAME:MASTER%的敏感部分、引导着射精……
					CASE 1
						PRINTFORML 「这样会很舒服吗……？」
						PRINTFORML 「快要射精的时候随时都可以……哇！」
						SIF !CFLAG:99:1998
							PRINTFORMDL 在丰姬说完之前、%CALLNAME:MASTER%就射出了精液
						PRINTFORML 「……唔……辛苦了、量好大啊」
						SIF !CFLAG:99:1998
							PRINTFORMDW 丰姬一边擦着沾在脸上的精液、一边对着%CALLNAME:MASTER%微笑……
					CASE 2
						PRINTFORML 「现在只要舒服起来就好了……」
						PRINTFORML 「所以不要忍耐……把自己托付给我……」
						IF !CFLAG:99:1998
							PRINTFORMDL 丰姬轻飘飘的话语、让%CALLNAME:MASTER%无意识地放松下来、马上就要射精了
							PRINTFORMDL 按照丰姬说的那样、放松全身的力量、委身于快乐之中、一转眼就射了出来……
						ENDIF
						PRINTFORMW 「……嗯♪ 真努力呢、了不起了不起」
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「放松下来就好……」
						PRINTFORML 「我会让你舒服的……」
						SIF !CFLAG:99:1998
							PRINTFORMDW 丰姬滴下唾液代替润滑剂、慢慢地開始撸动肉棒……
					CASE 1
						PRINTFORML 「你的这一根、还真是烫得不得了啊……」
						PRINTFORML 「如果这种东西插入我……然后」
						PRINTFORMW 「……什……什么啊！竟然会有这种妄想……？」
					CASE 2
						PRINTFORML 「哦呀？……再给我看看吧……？」
						PRINTFORML 「你舒服的表情……♪」
						SIF !CFLAG:99:1998
							PRINTFORMDW 丰姬一边凝视着因快乐而扭曲的%CALLNAME:MASTER%的脸、一边撸动着肉棒……
				ENDSELECT
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

