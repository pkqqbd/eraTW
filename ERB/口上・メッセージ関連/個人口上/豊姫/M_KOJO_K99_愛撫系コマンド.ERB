					ENDIF
				CASE 2
					CALL HPH_PRINT,@"「嗯……咦……哦……HPH」","L"
					CALL HPH_PRINT,@"「咕啾……HPH 真热情啊……HPH」","L"
					IF !CFLAG:99:1998
