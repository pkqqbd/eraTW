	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		;初回
			IF FIRSTTIME(1095)
				PRINTFORMW 「ねえ%CALLNAME:MASTER%、」
				PRINTFORMW 「我，好像有了喜欢的人了」
				PRINTFORMW 「那个人啊，温柔、帅气、又认真」
				PRINTFORMW 「消沉的时候，只要和那个人说说话，心里的阴霾好像一下就不见了」
				PRINTFORMW 「当我问姐姐这种时候该怎么办」
				PRINTFORMW 「『这得你自己决定』，给出了那样的答案」
				PRINTFORMW 「我啊，即使成为了付丧神，自己一个人也什么都做不到」
				PRINTFORMW 「永远都躲在姐姐和雷鼓姐的背后」
				PRINTFORMW 「但是，也不可能就这样一直下去」
				PRINTFORMW 「我有了比那两人更加重要的存在」
				PRINTFORMW 「%CALLNAME:MASTER%、」
				PRINTFORMW 「私は%CALLNAME:MASTER%のことが好き」
				PRINTFORMW 「世界中の誰よりも、%CALLNAME:MASTER%のことが大好き！」
;				PRINTFORMW 「わ、私は…その…姉さんや雷鼓さんや他のみんなみたいに華は無いけど…」
				PRINTFORMW 「我想成为你心中的第一位！」
				PRINTFORMW 「请和我成为恋人！」
			;二回目の告白
			ELSE
				PRINTFORMW 「あのね、%CALLNAME:MASTER%」
				PRINTFORMW 回程时，八桥像是下定决心似的開口了。
				PRINTFORMW 「看着我，哪怕只有一点点也好。」
				PRINTFORMW 八橋はそう言って%CALLNAME:MASTER%を振り向かせた。
				PRINTFORMW 「%CALLNAME:MASTER%と会えば会うほど好きになって」
				PRINTFORMW 「%CALLNAME:MASTER%と会わないと寂しくて」
				PRINTFORMW 「该怎么办啊…我不知道」
				PRINTFORMW 「无论如何…能做我的恋人吗…？」
			ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白オッケー（恋人が付く）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「…唔欸…」
		PRINTFORMW 八桥满脸通红地呜咽起来。
		PRINTFORMW 「嗯…谢谢你…」
		PRINTFORMW 八桥说着抱住了我。
		PRINTFORMW 「%CALLNAME:MASTER%…大好き…」
		PRINTFORMW %CALLNAME:MASTER%は八橋の頭を撫でながらしばらくそのままでいることにした。
		PRINTFORMW （就像这样…多来点…）
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW  「唔…嗯…」
		PRINTFORMW  「我，我知道了」
		PRINTFORMW  「じゃあね、%CALLNAME:MASTER%」
		PRINTFORMW  八桥说完就走了。
		PRINTFORMW  （如果，只是如果…）
		PRINTFORMW  （誰よりも早く…%CALLNAME:MASTER%に…会ってたら…）
		PRINTFORMW  （就更好了…）
		RETURN 1
	ENDIF

ENDIF
RETURN 1

;==================================================
;约会帰り
;==================================================
@M_KOJO_SPEVENT_K95_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_3(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF CFLAG:318 == 1
			PRINTFORMW 「…拜拜」
		ELSEIF TALENT:恋人
			PRINTFORM 「…今天过很開心，谢谢，
			CALL MASTER_CALLNAME_K95
			PRINTFORMW ！」
			PRINTFORMW 八橋はそう言いながら%CALLNAME:MASTER%の方へ向き直って礼を言った。
			PRINTFORM 「果然，和
			CALL MASTER_CALLNAME_K95
			PRINTFORMW 在一起真好啊」
			PRINTFORMW 「明天再约会吧！…约好了啊」
		ELSEIF TALENT:恋慕
			PRINTFORMW 「呵呵，今天好開心啊」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K95
			PRINTFORMW ，你觉得怎么样？」
			PRINTFORMW 「…もし、%CALLNAME:MASTER%さえよければ明日も―」
			PRINTFORMW 「…果然不行啊，再见了！」
			PRINTFORMW 八桥说着，依依不舍地挥挥手走了。
		ELSEIF TALENT:思慕
			PRINTFORMW 「再见，明天见！」
			PRINTFORMW 八桥说完就匆匆忙忙地回去了。
			PRINTFORMW （…如果…明天能见面的话…）
			PRINTW
		ELSE
			PRINTFORMW 「啊…我要回去了」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K95
			PRINTFORMW 还是早点睡比较好，熬夜对身体不好」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋人
			PRINTFORMW 「…明白了，这次就原谅你」
			PRINTFORMW 「但是呢…别再做坏事喽？」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K95
			PRINTFORMW 的话，我相信你哦？」
		ELSE
			PRINTFORMW 「嗯，可以啊」
			PRINTFORMW 「别再做惹怒别人的事了」
			PRINTFORMW 「我很温柔，这次就原谅你。」
			PRINTFORMW 八桥带着有些不自然的笑容这么说。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人
			PRINTFORMW 「…哼！」
			PRINTFORMW 「我还在生气呢！」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K95
			PRINTFORMW 的事我可不会那么轻易原谅的！」
		ELSE
			PRINTFORMW 「…还是很讨厌」
			PRINTFORM 「但是，
			CALL MASTER_CALLNAME_K95
			PRINTFORMW ，我就稍微原谅你…」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋人
			PRINTFORMW 「…还是很讨厌」
			PRINTFORM 「但是，
			CALL MASTER_CALLNAME_K95
			PRINTFORMW ，我就稍微原谅你…」
			PRINTFORMW 「…另外！」
			PRINTFORMW 「我不会一直生气的…」
		ELSE
			PRINTFORMW 「…好啊」
			PRINTFORMW 「只有一点点…原谅你！」
		ENDIF
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

