;==================================================
@M_KOJO_EVENT_K79_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;出入りする度に表示されるのは不自然なので表示は一日一度のみ
		SIF TCVAR:79:357 == 1
			RETURN 1
		;MASTERと雛が[706]厄神的家で同棲している場合(帰宅シーン想定)
		IF CFLAG:MASTER:初期位置 == 706 && CFLAG:79:初期位置 == 706 && CFLAG:MASTER:現在位置 == 706
			IF TALENT:恋人
				PRINTFORML 「欢迎回来，%CALLNAME:MASTER%。
				PRINTFORMW 　是要先吃饭？ 还是先洗澡？ 还是……先·吃·我？
				PRINTFORMW 　……试着说了一下、果然好羞耻啊、那个……」
			ELSE
				PRINTFORML 「%CALLNAME:MASTER%，欢迎回来。
				PRINTFORMW 　不在家的时候，没什么特别的变化」
			ENDIF
		;雛が博麗神社の[79]破舊小屋に住み込んでいて、MASTERと同棲している場合(帰宅シーン想定)
		ELSEIF FLAG:住宿人物 == 79 && CFLAG:MASTER:初期位置 == 16 && CFLAG:MASTER:現在位置 == 16
			IF TALENT:恋人
				PRINTFORML 「欢迎回来，%CALLNAME:MASTER%。
				PRINTFORML 　吃饭和洗澡都在社务所解决了……、嗯……、那个…
				PRINTFORMW 　果然、还是算了……！」
			ELSE
				PRINTFORML 「回来了呢、%CALLNAME:MASTER%。
				PRINTFORMW 　社务所有发生什么事吗？」
			ENDIF
		;それ以外
		ELSE
		
		ENDIF
		TCVAR:79:357 = 1
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		;出入りする度に表示されるのは不自然なので表示は一日一度のみ
		SIF TCVAR:79:358 == 1
			RETURN 1
		;MASTERと雛が[706]厄神的家で同棲している場合(帰宅シーン想定)
		IF CFLAG:MASTER:初期位置 == 706 && CFLAG:79:初期位置 == 706 && CFLAG:MASTER:現在位置 == 706
			IF TALENT:恋慕
				PRINTFORML 「%CALLNAME:MASTER%，我回来了。
				PRINTFORMW 　不在家的时候有什么变化么？」
			ELSE
				PRINTFORML 「我回来了、%CALLNAME:MASTER%。
				PRINTFORMW 　看家，辛苦了」
			ENDIF
		;雛が博麗神社の[79]破舊小屋に住み込んでいて、MASTERと同棲している場合(帰宅シーン想定)
		ELSEIF FLAG:住宿人物 == 79 && CFLAG:MASTER:初期位置 == 16 && CFLAG:MASTER:現在位置 == 16
			IF TALENT:恋慕
				PRINTFORML 「我回来了、%CALLNAME:MASTER%。
				PRINTFORMW 　……这样的话、好像夫妻一样呢」
			ELSE
				PRINTFORML 「哎呀、%CALLNAME:MASTER%。
				PRINTFORMW 　我说怎么在社务所找不到你、原来在这里啊」
			ENDIF
		;それ以外
		ELSE
		
		ENDIF
		TCVAR:79:358 = 1
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
