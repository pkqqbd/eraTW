			LOCAL:2 = M_KOJO_K103_KANKEI()
			SELECTCASE LOCAL:2
			CASE 10, 11, 12, 13, 21, 22, 23, 24
				;特別な感情は抱いていない時
				SELECTCASE M_KOJO_K103_RANDNR(4,6)
				CASE 1
					PRINTFORMW 「んっ……そこ、ぐりぐりされると…」
				CASE 2
					PRINTFORMW 「お腹の中から突き上げられてるみたい…っ」
				CASE 3
					PRINTFORMW 「…%M_KOJO_K103_AEGI()%…」
				CASEELSE
					PRINTFORMW 「奥ぐりぐりって……声出ちゃう……っ」
				ENDSELECT
				PRINTFORML
			CASE 31,32,33,34,61,62,63,64
				;思慕OR炮友
				SELECTCASE M_KOJO_K103_RANDNR(4,6)
				CASE 1
					PRINTFORMW 「はぁ…っ…んっ………」
				CASE 2
					PRINTFORMW 「あっ、おくっ、だめっ…だめっ…！」
				CASE 3
					PRINTFORMW 「あっ…あっ…腰勝手に動いちゃう……っ」
				CASEELSE
					PRINTFORMW 「お腹の中でそんなにあばれないでぇ……！」
				ENDSELECT
				PRINTFORML
			CASE 41,42,43,44,51,52,53,54,71,72,73,74
				;恋慕OR愛欲
				SELECTCASE M_KOJO_K103_RANDNR(4,6)
				CASE 1
					PRINTFORMW 「%M_KOJO_K103_AEGI()%…%M_KOJO_K103_AEGI()%…%M_KOJO_K103_HEART()%」
				CASE 2
					PRINTFORMW 「んんんんっ…おく来てる……っ…%M_KOJO_K103_HEART()%」
				CASE 3
					PRINTFORMW 「…あっ、そこ、そこぉ……！%M_KOJO_K103_HEART()%」
				CASEELSE
					PRINTFORMW 「%M_KOJO_K103_AEGI()%…%M_KOJO_K103_HEART()% ごりごりって…こ、腰止まんな……っ…！%M_KOJO_K103_HEART()%」
				ENDSELECT
				PRINTFORML
			ENDSELECT
		RETURN 1
	ENDIF
ENDIF
