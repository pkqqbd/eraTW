		RETURN 1
	ELSEIF DISHNAME == "野味菜"
		IF FIRSTTIME("固有料理練習")
			PRINTFORMW 「料理教えろって、私が教えられそうなことなんてある？」
			PRINTFORMW 「あ、シメ方とか捌き方なら教えられるかしら」
		ELSEIF STRCOUNT(DISH_TASTE, "練習")
			{
			PRINTFORMW 「%TEXTR("ここを上に引っ張ると関節が外れて蔷薇しやすくなるのよ/
			皮はここから首側に剥くと綺麗に剥けるわ/そこの軟骨切れば簡単に外れるわよ/
			深く切りすぎちゃ駄目。モツが破けちゃうわ")%」
			}
		ELSE
			PRINTFORMW 「捌き方、堂に入ってきたわね」
		ENDIF
		RETURN 1
	ENDIF
	IF STRCOUNT(DISH_TASTE, "香料/")
		PRINTFORML 「わっ！香辛料入れないで！」
		PRINTFORML 香料抜きで作りますか？
		CALL ASK_YN
		IF !RESULT
			DISH_TASTE '= REPLACE(DISH_TASTE, "香料\/", "")
			DISH_TASTE += "香料抜き/"
			PRINTFORMW 「ありがと……」
		ELSE
			PRINTFORMW 「鼻がビリビリする～……」
		ENDIF
		PRINTFORML 
	ENDIF
	;恋慕
	IF TALENT:恋慕
		;調理前口上
		SELECTCASE DISH_NAME
		;最初に例外群
		;"澳洲鴕鳥肉"はこっちに移動
		CASE "奶酪番茄沙拉", "興奮劑高湯", "澳洲鴕鳥肉"
			$KINDAN
			PRINTFORMW 「总……总觉得漂着一股異様的气息呢……」
		CASE "银吉獣料理"
			PRINTFORMW 「银吉獣到底是什么呢」
		
		;お魚系、天婦羅は唯一、魚料理がつかないので先に分岐
		;まー、TASTEにはついてないが内実は想像にまかせるということで
		CASE "天麩羅"
			$OSAKANA
			PRINTFORMW 「魚貝料理呢！」
			PRINTFORMW 「除了肉以外我最喜欢的就是魚了」
		;主食肉
		;青蛙料理もこの区分だが……
		;だってこの娘、蛙肉だろうが喜んで食いそうなんだもん
		CASE "漢堡肉", "涮涮锅", "姜汁焼肉", "牛扒", "油淋鶏", "炸青蛙", "青蛙咖喱", "炒青蛙", "盐烤青蛙"
			PRINTFORMW 「哇～♪是肉～♪」
			PRINTFORMW 「拿出气势做出来吧！」
		;軽食肉
		CASE "漢堡", "焼麥", "肉包"
			$ONIKU
			PRINTFORMW 「肉！肉多放一点吧！」
		;お手軽系、主食系を先に判定して軽食系はあとでまとめて判定します
		CASE "雑燴粥", "蛋澆飯", "盛的滿滿的飯"
			$OTEGARU
			PRINTFORMW 「啊啦、这种程度一下子就能做好呢」
			GOTO COOKINGEND
		[SKIPSTART]
		;仕様変更とともに闇に消えたっぽいのでコメントアウト
		;啤酒毛豆
		CASE 79
			PRINTFORMW 「夏天果然得配这两样～♪」
			PRINTFORMW 「…不过这不需要料理吧？」
			GOTO COOKINGEND
		;关东煮
		CASE 86
			PRINTFORMW 「提到冬天的话当然就是关东煮喽～♪」
			PRINTFORMW 「%CALLNAME:MASTER%喜欢哪一样呢？」
			PRINTFORMW 「我的话呢……」
		[SKIPEND]
		
		CASEELSE
			IF STRCOUNT(DISH_TASTE, ESCAPE("跳蛋/"))
				GOTO KINDAN
			ELSEIF STRCOUNT(DISH_TASTE, ESCAPE("麺類/"))
				PRINTFORMW 「在制作面的料理吗？」
				PRINTFORMW 「那么最後把蛋放进去做成月见面吧♪」
				DISH_TASTE += "卵/赏月/"
			ELSEIF STRCOUNT(DISH_TASTE, ESCAPE("魚料理/"))
				GOTO OSAKANA
			ELSEIF STRCOUNT(DISH_TASTE, ESCAPE("肉料理/"))
				GOTO ONIKU
			ELSEIF STRCOUNT(DISH_TASTE, ESCAPE("甜点/"))
				PRINTFORMW 「点心吗～。嗯、好羡慕呢」
				PRINTFORMW 「我也能学做这个吗…？」
			ELSEIF STRCOUNT(DISH_TASTE, ESCAPE("野菜/"))
				PRINTFORMW 「呜呜…」
				PRINTFORMW 「可以的话比起蔬菜我喜欢吃肉……」
				PRINTFORMW 「嘛%CALLNAME:MASTER%已经这样说了的话……」
				GOTO COOKINGEND
			ELSEIF STRCOUNT(DISH_TASTE, ESCAPE("軽食/"))
				GOTO OTEGARU
			ENDIF
		ENDSELECT
		PRINTL
		SELECTCASE RAND:6
		CASE 0
			PRINTFORMW 「呃嗯、把这个端起来…好烫！」
			PRINTFORMW 「呜呜…不太擅长对付火呢……」
		CASE 1
			PRINTFORMW 「秤？啊是那个呢」
			PRINTFORMW 「该怎么用呢？」
			PRINTFORMW 「啊！这东西真好玩！弹起来了呢！」
		CASE 2
			PRINTFORMW 「做这个就不用菜刀了吧……」
			PRINTL
			PRINTFORMW 嘶啦~嘶啦~
			PRINTL
			PRINTFORMW 「好像做的太粗糙了？」
		CASE 3
			PRINTFORMW 「呜ー……还得等吗～？」
			PRINTFORMW 「好想快点尝尝呢……」
		CASE 4
			PRINTFORMW 「料理虽然很费劲但真的很有趣呢♪」
			PRINTFORMW 「也许是因为和%CALLNAME:MASTER%在一起？」
		CASE 5
			PRINTFORML 「ねえねえ私に手伝えそうなことある？」
			PRINTFORML %CALLNAME:MASTER%の後ろをうろうろしている
		ENDSELECT
		$COOKINGEND
		PRINTL
		SIF ABL:MASTER:料理技能 > 2
			PRINTFORMW 「不愧是%CALLNAME:MASTER%呢。看起来非常美味的样子♪」

	;恋慕じゃない
	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「这种感覚是怎么回事呢？」
			PRINTFORMW 「要是能顺利做好就行了…」
		CASE 1
			PRINTFORMW 「大杯是差不多这样就是？还是这样？」
			PRINTL 
			PRINTFORMW 咕噜咕噜~
			PRINTL 
			PRINTFORMW 「……好像哪里弄错了？」
		CASE 2
			PRINTFORMW 「放入適量砂糖和料酒……」
			PRINTFORMW 「適量是多少啊！？」
		ENDSELECT
		PRINTL
		SIF ABL:MASTER:料理技能 > 2
			PRINTFORMW 「真是了不起的本领啊～。厉害厉害♪」
	ENDIF
ENDIF

RETURN 1
