;==================================================
@M_KOJO_MESSAGE_COM_K63_301
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K63_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K63_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && !FLAG:70 
	;初回
	IF FIRSTTIME(301)
		PRINTFORMW 「诶、这茶是泡给我喝的吗。谢谢」
		SELECTCASE DAY:2
		;夏
		CASE 2
			PRINTFORMW 「炎炎夏日能喝到冰茶真是沁入心扉呢～」
			PRINTFORMW 「不过热茶我也能喝得下去哦？」
		;夏じゃない
		CASEELSE
			PRINTFORMW 「是不是有点烫了？」
		ENDSELECT
			PRINTFORMW 「的确、野生的狼都是猫舌呢」
			PRINTFORMW 「但是觉得作为妖怪的狼人也会一样就大错特错了」
			PRINTFORMW 「不过为什么要叫做猫舌呢…咕咚咕咚……」

	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:6
		CASE 0
			PRINTFORMW 「不愧是%CALLNAME:MASTER%、动作很流畅呢♪」
			PRINTFORMW 「嗯～美味～♪」
		CASE 1
			PRINTFORMW 「哎呀谢谢♪我就不客气了…」
			PRINTL 呼~呼~嘶…
			PRINTFORMW 「因为人类会使用火所以没有猫舌呢」
			PRINTFORMW 「所以从自然的角度来看人类的舌头反而很奇怪呢」
			PRINTFORMW 「也就是说反之该称为人类舌、我有、%CALLNAME:MASTER%也有」
			PRINTFORMW 「啊嘞？不过我是妖怪所以得叫妖怪舌？」
		CASE 2
			PRINTFORMW 「呀啊～！正好喉咙干的要命呢！谢谢你%CALLNAME:MASTER%♪」
			PRINTFORMW 「因为喝茶不会醉、比起喝酒没有那么多顾虑」
			PRINTFORMW 「但是无论喝什么东西都要有节制哦」
			PRINTFORMW 「茶喝的太多也会变的睡不着的！」
		CASE 3
			PRINTFORMW 「之前就在想了……」
			PRINTFORMW 「这茶是什么时候泡好的」
			PRINTFORMW 「%CALLNAME:MASTER%偶尔也会做些不可思议的事情呢」
		CASE 4
			PRINTFORMW 「是%CALLNAME:MASTER%亲手泡的茶呢、好開心♪」
			PRINTFORMW 「一直以来真是多谢了♪」
		CASE 5
			SELECTCASE DAY:2
			CASE 1
				PRINTFORMW 「哎呀真少见呢、是抹茶呢。谢谢、我就不客气了」
				PRINTFORMW 「呜～、好像有点苦……」
			CASE 2
				PRINTFORMW 「夏天果然要喝麦茶呢」
				PRINTFORMW 「适度补充水分是不可或缺的！」
			CASE 3
				PRINTFORMW 「哎呀今天是红茶呢。谢谢、我就不客气了」
				PRINTFORMW 「唔嗯ー、不但味道不错、香气也很诱人呢」
			CASE 4
				PRINTFORMW 「哇啊～谢谢！」
				PRINTFORMW 「喝起来又暖和又美味呢～！」
			ENDSELECT
		ENDSELECT

	;恋慕じゃない
	ELSE
		SELECTCASE RAND:6
		CASE 0
			PRINTFORMW 「啊啦、动作很流畅呢。我就不客气了」
			PRINTFORMW 「嗯～美味～」
		CASE 1
			PRINTFORMW 「哎呀谢谢。我就不客气了」
			PRINTL 
			PRINTFORMW 「人类以外的動物全部都是猫舌呢」
			PRINTFORMW 「不过变成妖怪后还是不是就不得而知了」
		CASE 2
			PRINTFORMW 「嗯ー！正好喉咙干的要命呢。谢谢」
			PRINTFORMW 「茶与酒的美味是所有種族的共识呢」
		CASE 3
			PRINTFORMW 「之前就在想了……」
			PRINTFORMW 「这茶是什么时候泡好的」
			PRINTFORMW 「难道说是用了妖术？！」
		CASE 4
			PRINTFORMW 「一直以来都麻烦你了呢。非常感謝」
			PRINTFORMW 「呵呵、你的親切好像与茶水一起沁入我的五臓六腑了呢～」
		CASE 5
			SELECTCASE DAY:2
			CASE 1
				PRINTFORMW 「哎呀真少见呢、是抹茶呢。谢谢、我就不客气了」
				PRINTFORMW 「呜～、好像有点苦……」
			CASE 2
				PRINTFORMW 「夏天果然要喝麦茶呢」
				PRINTFORMW 「适度补充水分是不可或缺的！」
			CASE 3
				PRINTFORMW 「哎呀今天是红茶呢。谢谢、我就不客气了」
				PRINTFORMW 「唔嗯ー、不但味道不错、香气也很诱人呢」
			CASE 4
				PRINTFORMW 「哇啊～谢谢！」
				PRINTFORMW 「喝起来又暖和又美味呢～！」
			ENDSELECT
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
;==================================================
;302,身体接觸
;==================================================
@M_KOJO_MESSAGE_COM_K63_302
;成否判定
;TFLAG:192(-2=指令終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
IF TCVAR:発情
	IF TALENT:恋慕
		TFLAG:192 = 1
	ELSE
		TFLAG:192 = -1
	ENDIF
ELSEIF (TALENT:恋慕 || TALENT:思慕) && DAY > K63_EYEBROW && AT_HOME(MASTER)
	TFLAG:192 = 1
ELSE
	CALL TRAIN_MESSAGE
ENDIF
CALL M_KOJO_MESSAGE_COM_K63_302_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K63_302_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && !FLAG:70 
	;初回
	IF FIRSTTIME(302)
		PRINTFORMW 「等、等等！不要随便摸啊！」
		PRINTFORMW 「啊、对不起…冷不防的吃了一惊呢……」
		SIF TALENT:思慕
			PRINTFORMW 「嘛…%CALLNAME:MASTER%的话稍微碰一碰也没事……」
		RETURN 1
	ENDIF
	
	;恋慕（お風呂）
	IF TALENT:恋慕 && BATHROOM()
		PRINTFORMW 「要帮我擦背？」
		PRINTFORMW 「谢谢……」
		PRINTFORML 
		PRINTFORMW 「%TEXTR("那个…不要这么一直盯着看呀……/啊哈～♪好～、好痒啊～！嘻嘻～/好的、接下来轮到我了……")%」
		RETURN 1
	ENDIF
	IF TCVAR:発情
		FONTBOLD
		CALL HPH_PRINT,@"「呀嗯゛っっHPH」","W"
		FONTREGULAR
		IF TALENT:恋慕
			CFLAG:积攒度 += 100
			PALAM:TARGET:潤滑 += 500
			BASE:TARGET:情緒 += 500
			PRINTFORMW 「对、对不起……现在被碰到的话该说是有些困扰还是这么说呢……那个、总之不是讨厌你哟……」
			PRINTFORMW （呜呜、被%CALLNAME:MASTER%碰到的时候、身子擅自就为交尾做好准备了…）
			IF TALENT:自己愛 >= 0
				CFLAG:TARGET:同行 = 60
				CFLAG:MASTER:同行 = 60
				PRINTFORMW 「好啦……你也明白的吧？　呐、我们去那边吧？」
				PRINTFORMW %CALLNAME:TARGET%轻轻捏住%CALLNAME:MASTER%的袖子朝某个方向拉了拉
			ENDIF
		ELSEIF TALENT:思慕
			PRINTFORMW 「不、不好意思……我想暂时独处一会、还请不要来碰我……」
		ELSE
			PRINTFORMW 「不要擅自摸过来啊！离远点！」
			CFLAG:ブチギレ = 1
		ENDIF
		CALL COLORMESSAGE(@"欲求不満上升了",C_YELLOW,1)
		CFLAG:积攒度 += 100
		IF PALAM:TARGET:潤滑 < 3000
			CALL COLORMESSAGE(@"潤滑值上升了",C_YELLOW,1)
			PALAM:TARGET:潤滑 += 500
		ENDIF
		RETURN 1
	ENDIF
	IF (TALENT:恋慕 || TALENT:思慕) && DAY > K63_EYEBROW && AT_HOME(MASTER)
		CALL K63_眉カット
		RETURN 1
	ENDIF
	CALL KAGEROU_SKINSHIP
	;恋慕
	IF TALENT:恋慕
		SELECTCASE RESULT
		;手を握る
		CASE 0
			IF !RAND:3
				PRINTFORML 「やっぱり%CALLNAME:MASTER%の手は私のとは違うわね」
				PRINTFORML 「道具を作って扱う、人类の手だわ」
			ELSEIF RAND:2
				PRINTFORMW 「啊…等等……」
				PRINTFORMW 「我的手、爪子立起来的时候是很危险的、要小心哦…」
			ELSE
				PRINTFORMW 「呵呵、%CALLNAME:MASTER%的体温隐隐约约的从手上传过来了呢……」
				PRINTFORMW 「这样牵手不是跟恋人一样了吗……」
				PRINTFORMW 「有点、不好意思呢……」
			ENDIF
		;頬を撫でる
		CASE 1
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「あっ…どしたの%CALLNAME:MASTER%……」
					PRINTFORML 「ふふ、私もしてあげる……」
				CASE 1
					PRINTFORML 「わっ！急にやられるとびっくりするわ」
					PRINTFORML 「つ、つねったりしないでね」
				CASE 2
					PRINTFORML 「ん……」
					PRINTFORML 目を細め、撫でている側に頭を傾けてきた
			ENDSELECT
		;耳に触る
		CASE 2
			IF RAND:2
				PRINTFORMW 「这也是真正的耳朵哦」
				PRINTFORMW 「就和妖兽们的耳朵是一个道理呢♪」
			ELSE
				PRINTFORMW 「这边的耳朵是可以动的呢、看～」
				PRINTFORMW 「想藏起来的时候就这样、让它趴下……」
			ENDIF
		;顎を擽る
		CASE 3
			IF RAND:2
				PRINTFORML 「あっ、それ気持ちいい……」
				PRINTFORML 「で、でもくすぐったいわ」
			ELSE
				PRINTFORML 「ふぁあ…くすぐったいわ……」
				PRINTFORML 「ああ…あと、犬みたいで恥ずかしい……」
			ENDIF
		;髪に触る
		CASE 4
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「ちゃんとお手入れはしてるつもりだけど…」
					PRINTFORML 「そうやってまじまじと観察されるとなんだか緊張するわね……」
				CASE 1
					PRINTFORML 「そんな嗅いだりしないでよ～…」
					PRINTFORML 「恥ずかしいわ……」
				CASE 2
					PRINTFORML 「手触り気持ちいい？」
					PRINTFORML 「ふふ、これにかけては稍微自信あるのよ」
			ENDSELECT
			PRINTFORML 艶々とした黒髪を堪能した…
		;肩を揉む
		CASE 5
			PRINTFORMW 「啊～好舒服哇～♪」
			IF RAND:2
				PRINTFORMW 「不是挺拿手的吗」
				PRINTFORMW 「是在灵梦那儿练出来的这个技巧吧？」
			ELSE
				PRINTFORMW 「接下来让我帮你揉揉吧」
				PRINTL 
				PRINTFORMW 「客人的肩膀可真是僵硬呢～」
				PRINTFORMW 「怎么样？我的手艺还行吗？」
			ENDIF
		;背を撫でる
		CASE 6
			IF RAND:2
				PRINTFORMW 「呼啊～……！」
				PRINTFORMW 「虽然有些痒痒的、但不知怎的就平静下来了呢」
			ELSE
				PRINTFORMW 「啊啊…总觉得心情好舒畅啊……」
				PRINTFORMW 「特别的安心…都有点想睡了呢……」
			ENDIF
		;脇腹をつつく
		CASE 7
			IF RAND:2
				PRINTFORMW 「啊哈哈！才不是让你突然挠痒痒啦！」
				PRINTFORMW 「%CALLNAME:MASTER%也来感受一下吧」
				PRINTFORMW 「看招～！看招看招～！」
			ELSE
				PRINTFORMW 「有点痒呢！啊哈哈！」
				PRINTFORMW 「呀啊～！够了快住手吧！」
				PRINTFORMW 「很羞人啊别弄了～！」
			ENDIF
		;鼻を擽る
		CASE 8
			IF RAND:2
				PRINTFORMW 「呜啊…痒痒的呢～……」
				PRINTFORMW 「啊嚏～！」
			ELSE
				PRINTFORMW 「呵呵、想干什么呢」
				PRINTFORMW 「%CALLNAME:MASTER%也感受一下吧」
				PRINTFORMW 「怎么样？是不是感到怪怪的呢？」
			ENDIF
		;ぺたぺた
		CASE 9
			PRINTFORMW 「从刚才開始就缠着我想干什么啊」
			IF RAND:2
				PRINTFORMW 「虽然并不是很讨厌、感覚还可以…」
				PRINTFORMW 「但总觉得哪里怪怪的呢…%CALLNAME:MASTER%」
			ELSE
				PRINTFORMW 「这么想摸的话…那么……」
				PRINTFORMW 「去稍微隐蔽一点的地方吧……」
			ENDIF
		CASE 10
			IF FIRSTTIME("肉球")
				PRINTFORMW 「肉球触りたい？まあ、変身すればあるけど……」
				PRINTFORML 影狼は右手を左袖に入れて、もぞもぞと動かす
				PRINTFORML そして差し出された右手は狼のそれだ
				PRINTFORMW 「是、どうぞ」
				PRINTFORML 柔らか……くはない、靴底みたいだ
				PRINTFORMW 「姫たちにも勝手にガッカリされたのよねー……」
			ELSE
				PRINTFORML 「%TEXTR("何が面白いんだか……/また？しょうがないわね……")%」
				PRINTFORML 変身した手を触らせてもらった
				IF !RAND:3
					PRINTFORML 「片手だけ獣だと手套の絵本みたい？そういう話があるの？」
					PRINTFORML 「あ、狐なのね……狼じゃないんだ……」
				ELSE
					PRINTFORML %TEXTR("普段の女性らしい手とはまるで違うが、これも影狼だ/地面に爪を立て蹴るための、力強い獣の前肢だ")%
				ENDIF
			ENDIF
		;人狼
		CASE 100
			CALL ERATOHO_JINROU_GAME
		ENDSELECT
	;恋慕じゃない
	ELSE
		SELECTCASE RESULT
		;手を握る
		CASE 0
			PRINTFORMW 「啊啦、你的手很漂亮呢」
			PRINTFORMW 「毎天都有护理吗？」
		;頬を撫でる
		CASE 1
			PRINTFORMW 「干嘛干嘛干嘛ー！？」
			PRINTFORMW 「真是的、老是冷不防的这样吓人一跳呢！」
		;耳に触る
		CASE 2
			PRINTFORMW 「这也是真正的耳朵哦」
			PRINTFORMW 「原理是什么？哼哼哼、秘密♪」
		;顎を擽る
		CASE 3
			PRINTFORMW 「……这是、把我当狗吗？」
			PRINTFORMW 「要生气了哦？」
		;髪に触る
		CASE 4
			PRINTFORMW 「在对我的头髪作什么呢……」
			PRINTFORMW 「嘛头髪可是女人的生命呢」
			PRINTFORMW 「怎么了嘛、妖怪说这种话很奇怪吗？」
		;肩を揉む
		CASE 5
			PRINTFORMW 「啊ー真不错呢。好舒服」
			PRINTFORMW 「再多帮我揉一会儿吧～」
		;背を撫でる
		CASE 6
			PRINTFORMW 「呀啊啊！干什么啊真是的」
			PRINTFORMW 「背上都发毛了呢」
		;脇腹をつつく
		CASE 7
			PRINTFORMW 「哇哇～！你在干什么啊！」
			PRINTFORMW 「可恶～！可恶～！」
		;鼻を擽る
		CASE 8
			PRINTFORMW 「呜啊……干什……啊嚏～！」
			PRINTFORMW 「真是的ー！搞什么啊ー！」
		;ぺたぺた
		CASE 9
			PRINTFORMW 「从刚才開始就缠着我想干什么啊」
			PRINTFORMW 「既然你住在神社里、妖怪什么的对你来说也没那么稀罕吧」
			PRINTFORMW 「再做这样的事要是被吃掉了可别说我没警告过你哦？」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
