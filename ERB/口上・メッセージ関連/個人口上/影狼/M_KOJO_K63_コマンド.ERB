;==================================================
@M_KOJO_MESSAGE_COM_K63_350
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K63_350_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K63_350_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && !FLAG:70 
	;单方面地「抱かれたい」「你のモノにされたい」のではなく双方向性を強調
	IF !TCVAR:初次拥抱
		IF TALENT:63:自己愛 < 0
			PRINTFORML 「%TEXTR("あっ……したいの？/えっと……お手柔らかに、お願いね/また？嫌じゃない、けど")%」
		;ELSEIF TALENT:MASTER:濃厚精液
		ELSEIF BASE:情緒 < 500
			CALL PRINT_FACE, [[影狼]], "憤怒","服", "",""
			PRINTFORML 「もう稍微雰囲気盛り上げてよ」
			PRINTFORML 「ただ交尾するだけなら虫でもできるわよ」
		ELSEIF TCVAR:発情 || CFLAG:积攒度 >= 800
			TCVAR:発情 = 1
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「うん、私も、愛を確かめ合いたい気分……HPH」","W"
				CASE 1
					CALL HPH_PRINT,@"「ん～～～～～～～～～っHPH　たまんない……HPH」","W"
					PRINTFORML 影狼は%CALLNAME:MASTER%の脖子に顔を埋め、オスの体臭を目一杯吸い込む
					PRINTFORML うなじを舐め上げ、柔らかな胸を、しなやかな腹を、服越しに押し付ける
					PRINTFORML 「%CALLNAME:MASTER%も私のこと、感じて？」
				CASE 2
					PRINTFORMW 「ふふ、そんなに私が欲しい？私もよ」
					CALL HPH_PRINT,@"「ね、わかる？　%CALLNAME:MASTER%が欲しくて、カラダ、熱くなってるの……HPH」","W"
					PRINTFORML 言葉街道影狼の体温は常にも増して高く、肌は汗ばみ吐息は荒い
			ENDSELECT
		ELSEIF CFLAG:积攒度 < 250
			PRINTFORML 「%TEXTR("いいけど、雑なのは嫌よ？/また？人类はサカりっぱ無ね")%」
		ELSE
			SELECTCASE RAND:4
				CASE 0
					PRINTFORML 「こら、がっつかないの」
					PRINTFORML 「求愛ならそれなりの作法があるでしょ？」
					CALL ASK_YN("摸着脸蛋并髪を撫でる","跪いて手の甲に接吻")
					CALL HPH_PRINT,@"「んふふ……HPH　ごうかく……HPH」","L"
				CASE 1
					PRINTFORML 「よかった」
					PRINTFORML 「誘ってくれなかったら、私から誘おうと思ってたの」
				CASE 2
					PRINTFORML 「うん、待ってた……」
				CASE 3
					PRINTFORML 「ふふ」
					PRINTFORML 「求められるって、いいわね」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
