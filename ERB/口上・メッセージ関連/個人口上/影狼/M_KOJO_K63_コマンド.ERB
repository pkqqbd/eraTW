RETURN RESULT

@M_KOJO_MESSAGE_COM_K63_412_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;勉強は嫌い
IF LOCAL && !FLAG:70 
	;初回
	IF FIRSTTIME(412)
		PRINTFORMW 「在学习？真了不起呢」
		PRINTFORMW 「诶！？我也要一起吗！？」
		PRINTFORMW 「唔ー…虽然觉得没什么必要」
		PRINTL 
		SIF ABL:MASTER:教養 > ABL:教養
			PRINTFORMW 「哇～、%CALLNAME:MASTER%真是博学呢」
	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:5
		CASE 0
			PRINTL
			PRINTFORMW 「呐%CALLNAME:MASTER%、差不多该休息一下了…」
			PRINTFORMW 「呐呐、呐～唉～……」
		CASE 1
			PRINTFORMW 「%CALLNAME:MASTER%真是热心呢」
			PRINTFORMW 「不过不好意思我对这个完全不在行……」
		CASE 2
			PRINTFORMW 「两个圆相交的交点…？？？」
			PRINTFORMW 「！」
			PRINTFORMW 「这个看起来好像月亮啊！」
		CASE 3
			PRINTFORMW 「啊、这是"幻想乡縁起"呢」
			PRINTFORMW 「我有读过一次呢」
		CASE 4
			PRINTFORMW 「百人一首问答！」
			PRINTFORMW 「我出上句、你对下句吧♪」
			PRINTL 
			$QUIZ_01
			PRINTL [1]清夜子规啼、声声碎耳旁。
			PRINTL [2]夏宵意正浓、不觉曦已至
			PRINTL [3]浮世愁难却、不得遂本心
			INPUT
			SIF !RESULT || RESULT > 3
				GOTO QUIZ_01
			LOCAL:1 = RESULT
			$QUIZ_02
			PRINTL 
			PRINTL [1]云间何处隐、晓月已无形　
			PRINTL [2]遍寻空不见、残月晓天长。
			PRINTL [3]红尘若有恋、夜半明月间　
			INPUT
			SELECTCASE RESULT
			CASE 1
				IF LOCAL:1 == 2
					PRINTFORMW 「Bingo♪」
					PRINTFORMW 「是咏叹夏夜苦短的和歌呢」
				ELSE
					PRINTFORMW 「嘟嘟ー！真可惜答错了！」
					GOTO QUIZ_BATSU
				ENDIF
			CASE 2
				IF LOCAL:1 == 1
					PRINTFORMW 「Bingo♪」
					PRINTFORMW 「是描述遍寻杜鹃鸣叫却只找得到月亮的和歌呢」
				ELSE
					PRINTFORMW 「嘟嘟ー！真可惜答错了！」
					GOTO QUIZ_BATSU
				ENDIF
			CASE 3
				IF LOCAL:1 == 3
					PRINTFORMW 「Bingo♪」
					PRINTFORMW 「只要活在世上就会想起那轮明月～这样的和歌呢」
				ELSE
					PRINTFORMW 「嘟嘟ー！真可惜答错了！」
					GOTO QUIZ_BATSU
				ENDIF
			CASEELSE
				GOTO QUIZ_02
			ENDSELECT
		ENDSELECT
		IF ABL:MASTER:教養 > ABL:教養
			PRINTL 
			PRINTFORMW 「%CALLNAME:MASTER%真的很博学呢～好感动啊」
			$QUIZ_BATSU
		ENDIF
	;恋慕じゃない
	ELSE
		SELECTCASE RAND:5
		CASE 0
			PRINTL …
			PRINTFORMW 「呐、不休息一下吗？」
			PRINTFORMW 「啊、是…这样……吗…」
		CASE 1
			PRINTFORMW 「热心学习是件好事。不过…」
			PRINTFORMW 「让我陪读的话有点困扰呢」
		CASE 2
			PRINTFORMW 「我说……」
