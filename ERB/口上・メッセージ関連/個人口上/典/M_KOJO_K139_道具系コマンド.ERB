;==================================================
;50,戴上套套
;==================================================
@M_KOJO_MESSAGE_COM_K139_50
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K139_50_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K139_50_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:恋慕 || TALENT:恋人
		IF TCVAR:発情
			SETCOLOR 0xFF69B4
			CALL HPH_PRINT,@"「诶诶～HPH　这里正干劲十足的想要被授精呢HPH　来不戴套直接做吧HPHHPH」","L"
			RESETCOLOR
		ELSE
			IF TALENT:妊娠願望
				PRINTFORML 「诶诶～～、不是已经决定好两人一起来造小孩了吗」
			ELSE
				IF TALENT:MASTER:万能孕 || TALENT:MASTER:讓妖怪懷孕
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%阁下的种子的话、只要一发就能让典怀孕呢…HPH」","L"
					SIF TALENT:MASTER:濃厚精液
						CALL HPH_PRINT,@"「可是、套套会不会破呢、典有些担心…HPH」","L"
				ELSE
					SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「不生孩子、纯粹只想体验性爱的愉悦呢…HPH」","L"
						CALL HPH_PRINT,@"「呵呵、那就不停地做到整箱套套全用完为止吧HPH」","L"
					CASE 1
						CALL HPH_PRINT,@"「虽说典就算怀上%CALLNAME:MASTER%阁下的孩子的话也不会觉得困扰…HPH」","L"
						PRINTFORML 「但%CALLNAME:MASTER%阁下不会那么做吧」
					ENDSELECT
				ENDIF
			ENDIF
		ENDIF
		PRINTW 
		RETURN 1
	;それ以外
	ELSE
		IF TEQUIP:PLAYER:避孕套
			IF ESTRUS_CYCLE(TARGET) == 15
				PRINTFORML 「今天就算不戴明明也没关系的…很担心怀孕吗？」
			ELSE
				CALL HPH_PRINT,@"「呵呵、我、也不讨厌戴着避孕套做哟HPH」","L"
				CALL HPH_PRINT,@"「特意戴上避孕套就是说、不是以生孩子作为目的、而是单纯想做爱啊HPH」","L"
				CALL HPH_PRINT,@"「啊啊、也就是说、想侵犯我想得ー不得了ー这样子、光是想象一下就湿了啊…HPH」","L"
			ENDIF
		ELSE
			CALL HPH_PRINT,@"「果然、感觉还是想要不戴套做呢…HPH」","L"
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1