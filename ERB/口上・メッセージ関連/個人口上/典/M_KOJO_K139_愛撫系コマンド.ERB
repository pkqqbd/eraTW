			CALL HPH_PRINT,@"「真是的、%CALLNAME:MASTER%阁下你啊、真是没出息哦HPH」","L"
			CALL HPH_PRINT,@"「加把劲HPH　加把劲HPH　肉棒要加把劲啊HPH」","L"
			RESETCOLOR
			PRINTW 
			PRINTFORML 典用嘴喂你喝了某种奇怪的液体
			PRINTFORML 疲劳的身体变轻松了一些…
			PRINTW 
			BASE:MASTER:体力 = MAXBASE:MASTER:体力
			BASE:MASTER:気力 = MAXBASE:MASTER:気力
			BASE:MASTER:精力 = MAXBASE:MASTER:精力
			BASE:MASTER:勃起 = MAXBASE:MASTER:勃起
			BASE:体力 = MAXBASE:体力
			BASE:気力 = MAXBASE:気力
			SETCOLOR 0xFF69B4
			CALL HPH_PRINT,@"「接下来还要继续加油哦HPH」","L"
			CALL HPH_PRINT,@"「来HPH　继续来造小孩吧HPH」","L"
			PRINTW 
			TCVAR:360 += 1
		ELSE
			IF TCVAR:発情
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「这就结束了真讨厌啊HPH　来继续做嘛HPHHPH」","L"
				IF HAS_PENIS(MASTER)
					CALL HPH_PRINT,@"「加把劲HPH　加把劲HPH　肉棒要加把劲啊HPH」","L"
					RESETCOLOR
					PRINTFORML 典对着变得半勃的阴茎如同小雨润田一般亲吻着…
				ENDIF
				PRINTW 
			ELSE
				SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「呵呵、真是好喜欢、这样摸着这个半勃着的肉棒…%UNICODE(0x2665) *1%」
					PRINTFORML 典把半勃起的肉棒放在手心上把玩着…
					PRINTW 
				CASE 1
					PRINTFORML 「累了吗？　稍稍、休息一下吧」
					PRINTFORML 典一脸温柔地抚摸着%CALLNAME:MASTER%的头
					PRINTW 
				ENDSELECT
			ENDIF
		ENDIF
		RETURN 1
	;それ以外
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「呵呵、好好一起享受吧」
		CASE 1
			PRINTFORML 「哦呀哦呀、已经歇了吗？」
		ENDSELECT
		PRINTW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

