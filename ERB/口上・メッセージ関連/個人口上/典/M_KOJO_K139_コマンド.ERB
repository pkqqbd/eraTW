;==================================================
;189,新妻PLAY
;==================================================
@M_KOJO_MESSAGE_COM_K139_189
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K139_189_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K139_189_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;服を着てない（ノーブラ・胖次ありはブラを自由に変更できないので多分できない）
		IF TEQUIP:5 && TEQUIP:0 & 0
			PRINTFORML 「这就是传说中的『裸体围裙』吗…」
			;内衣あり
			IF EQUIP:下半身内衣１ || EQUIP:下半身内衣２
				PRINTFORML 「嘛、不过我还穿着内衣」
				PRINTFORML 「还是说把内衣也脱了会比较好呢？　亲・爱・的%UNICODE(0x2665) *1%」
				PRINTW 
			ELSE
				PRINTFORML 「总觉得心里不踏实啊」
				PRINTFORML 「难道说、你要我穿成这个样子做家务吗？」
				CALL HPH_PRINT,@"「不行、穿成这样打扫的话、重要的地方就全都暴露了…HPH」","L"
				CALL HPH_PRINT,@"「然后、很可能会被失去理智的%CALLNAME:MASTER%阁下给袭击…HPH」","L"
				CALL HPH_PRINT,@"「如果那样的话…典很可能会因为无法抵抗而被推倒…HPH」","L"
				PRINTW 
			ENDIF
		ELSE
			PRINTFORML 「喔喔、这可真是新鲜啊」
			PRINTFORML 「总的来说、我是属于烹饪派的」
			PRINTFORML 「这样确实很有新婚妻子的感觉呢%UNICODE(0x2665) *1%」
			PRINTW 
			PRINTFORML 「所以说、固定的那个、要做吗？」
			CALL ASK_YN("请给我最好的","大丈夫、萌大奶")
			IF !RESULT
				PRINTFORMW 「我知道了♪」
				PRINTFORML 「你今天也辛苦了%UNICODE(0x2665) *1%」
				PRINTFORML 「是先吃饭呢？　还是先洗澡呢？　还是、先・吃・我？」
				PRINTW 
			ELSE
				PRINTFORML 「啊啊、真是爱使坏啊」
				PRINTFORML 典双手交叉在胸前、身体忸怩作态地摇晃者
				PRINTW 
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;開始
	IF TEQUIP:新妻PLAY
		PRINTFORML 「呼呼、又来了吗？」
		IF TEQUIP:5 && TEQUIP:0 & 0
			IF DAY:2 == 4
				PRINTFORML 「好冷！　好冷啊！」
				PRINTFORML 典抱着身体、不断地颤抖着
				PRINTW 
			;内衣あり
			ELSEIF EQUIP:下半身内衣１ || EQUIP:下半身内衣２
				SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「不脱内衣的话、总觉得你有什么狂热的癖好啊」
				CASE 1
					PRINTFORML 「不脱内衣是有什么讲究吗？」
					PRINTFORML 「对典来说、只要%CALLNAME:MASTER%阁下高兴的话、让我做什么都可以」
				ENDSELECT
				PRINTW 
			ELSE
				SELECTCASE RAND:3
				CASE 0
					IF TALENT:TARGET:胸圍 >= 0
						SELECTCASE RAND:3
						CASE 0
							PRINTFORML 「%CALLNAME:MASTER%阁下、典的裸体围裙、怎么样？」
							PRINTFORML 典为了突出胸部、特意采取了前倾的姿势
						CASE 1
							PRINTFORML 「我的胸都快从围裙里露出来了…」
							PRINTFORML 「太过分了、根本就藏不住啊」
						CASE 2
							SELECTCASE RAND:2
							CASE 0
								PRINTFORML 因为典的胸并不是很大、隐约可见樱色的乳突…
								PRINTFORML 「每当胸被摩擦到的时候…总会有种奇怪的感觉…%UNICODE(0x2665) *1%」
							CASE 1
								PRINTFORML 「呼呼、从侧面看到的胸部是不是别有一番风味呢♪」
							ENDSELECT
						ENDSELECT
					ELSE
						PRINTFORML 「要是我的胸再大点的话、就可以把%CALLNAME:MASTER%阁下给迷得神魂颠倒吧…」
						PRINTFORML 我告诉典、从围裙的缝隙中看到的樱花色乳突才是至高无上的
						CALL HPH_PRINT,@"「真是的…%CALLNAME:MASTER%阁下、真是H呢…HPH」","L"
					ENDIF
					PRINTW 
				CASE 1
					PRINTFORML 「呼呼、不能看我的屁股哦%UNICODE(0x2665) *1%」
					PRINTFORML 典为了让我着急起来、故意用尾巴挡住了屁股
				CASE 2
					PRINTFORML 「打扫的时候、不能从后面偷看哦？」
				ENDSELECT
				PRINTW 
			ENDIF
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「还特意换上这个啊…」
				PRINTFORML 「这就是所谓的『角色扮演』吧？」
			CASE 1
				PRINTFORML 「亲爱的、今天请多多关照哦…%UNICODE(0x2665) *1%」
			CASE 2
