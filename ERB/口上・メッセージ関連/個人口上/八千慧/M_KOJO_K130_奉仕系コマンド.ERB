@M_KOJO_MESSAGE_COM_K130_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K130_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K130_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「こんな汚いモノを私に握らせるだなんて……」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「もう出そうなんですか？情けないですね%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「ふふっだらしない顔をして、出しちゃえばいいじゃないですか%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「ん、もう出そうなんですか？」
		ENDIF
	;技巧4以上
	ELSEIF ABL:技巧 > 3
		IF RAND:3 == 0
			PRINTFORMW 「手扱きぐらい造作もありませんよ%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「んっ%UNICODE(0x2665) *1%早く出しなさい%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「手の方がいいだなんて不能なのですか%UNICODE(0x2665) *1%」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「全くこんなことをさせるだなんて……」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「汚いフニャチンですね……これでも使い物になるんですか？」
		ELSE
			PRINTFORMW 「んー早く出してくれませんかね」
		ENDIF
	ENDIF
ENDIF
RETURN 1

