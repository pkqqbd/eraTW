					PRINTFORMW 
				ENDIF
			CASE 6
				IF ESTRUS_CYCLE(TARGET) == 150
					PRINTFORML 「哈啊……%UNICODE(0x2764)%明明是危险日却被这么猛烈地中出的话%UNICODE(0x2764)%」
					PRINTFORML 「会怀上孩子的啊…%UNICODE(0x2764)% %CSTR:14:20%的身体被%CALLNAME:MASTER%这样子搞的话…要坏掉了啊%UNICODE(0x2764)%」
					PRINTFORMW 
				ELSE
					PRINTFORML 「哈啊,好厉害啊……%UNICODE(0x2764)%%CALLNAME:MASTER%浓浓的东西,就这么在%CSTR:14:20%的里面出来了%UNICODE(0x2764)%」
					PRINTFORML 「哈啊…真是舒服啊…%UNICODE(0x2764)%」
					PRINTFORMW 
				ENDIF
			CASE 7
				PRINTFORML 「哈啊%UNICODE(0x2764)% 哈啊…%UNICODE(0x2764)% 琪露诺的里面充满了来自%CALLNAME:MASTER%的东西啊……%UNICODE(0x2764)%」
				PRINTFORML 「热热的感觉在小腹里逐渐扩散开来了……要融化了啊%UNICODE(0x2764)%」
				PRINTFORMW 
			CASE 8
				PRINTFORML 「哈啊…%UNICODE(0x2764)% 把好多热热的东西注入进来了啊%UNICODE(0x2764)%」
				PRINTFORML 「……真是太厉害了啊%UNICODE(0x2764)%%UNICODE(0x2764)%%UNICODE(0x2764)%」
				PRINTFORMW 
			CASE 9
				PRINTFORML 「嗯…%UNICODE(0x2764)% 真是的%UNICODE(0x2764)% 哈啊%UNICODE(0x2764)% 好多热热的东西在%CSTR:14:20%的里面扩散开来了啊…%UNICODE(0x2764)%」
				PRINTFORML 「%CALLNAME:MASTER%に爱的证明,%CSTR:14:20%完完全全的感受到了哟%UNICODE(0x2764)%」
				PRINTFORMW
 			CASEELSE
				IF EX:膣内精液 >= 1200
					PRINTFORML 「好,厉害%UNICODE(0x2764)% %CALLNAME:MASTER%的精液,要让琪露诺琪露诺的肚子……鼓起来了%UNICODE(0x2764)%」
					PRINTFORML 「……%CALLNAME:MASTER%真是hentai呢%UNICODE(0x2764)%」
					PRINTFORMW 
				ELSE
					PRINTFORML 「啊……%UNICODE(0x2764)%%CALLNAME:MASTER%在%CSTR:14:20%的小穴里射进来了……喜欢%UNICODE(0x2764)%」
					PRINTFORMW 
				ENDIF
			ENDSELECT
		ENDIF
	ELSE
		IF TALENT:MASTER:濃厚精液
			IF RAND:3 == 0
				PRINTFORML 「啊,……里面……射出来了」
				PRINTFORMDL  琪露诺用空虚的目光注视着持续射精的交合部
				PRINTFORMW 
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORML 「哈……啊……要怀孕了……」
				PRINTFORMW 
				RETURN 1
			ELSE
				PRINTFORML 「啊……啊……还在,射……出来……」
				PRINTFORMW 
				RETURN 1
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORML 「啊,哈……肚子里,被咕嘟咕嘟地灌进来了」
				PRINTFORML 「……热度……仿佛渗透进,身体里了」
				PRINTFORMW 
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORML 「是……啊,厉害……」
				PRINTFORMW 
				RETURN 1
			ELSE
				PRINTFORML 「哈……哈……是……啊!」
				PRINTFORML 「厉害……还在射进,%CSTR:14:20%的小穴里……」
				PRINTFORMW 
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1
