;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K104_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K104_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K104_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;特殊状況
	IF FLAG:70 ;時姦
		PRINTFORMW 
		RETURN 1
	ELSEIF CFLAG:MASTER:恶作剧 == 2
		PRINTFORMW 
		RETURN 1
	ELSEIF TCVAR:烂醉 == 1
		IF TALENT:恋慕 || TALENT:愛欲
			PRINTFORML 「はぁ～い、しこしこ～」
			PRINTFORMW 「どーかなぁ、こんなかんじでいい～？」
		ELSE
			PRINTFORML 「あっはは、ぐりぐり～」
			PRINTFORMW 「う～ん、なんかすごいねぇ」
		ENDIF
		RETURN 1
	;通常
	ELSE
		IF TALENT:恋慕 || TALENT:愛欲
			PRINTFORML 「用手吗？　嗯，交给我吧」
			PRINTFORMW 「我会，好好地让%CALLNAME:MASTER%舒服起来的……%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORML 「要怎么做才会比较舒服一点呢？」
			PRINTFORMW 「一步一步的来尝试一下吧～」
		ENDIF
	ENDIF
ENDIF
RETURN 1

