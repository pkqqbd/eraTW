;==================================================
;约会帰りに告白（恋人が付く）
;==================================================
@M_KOJO_SPEVENT_K104_2(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_2(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;告白内容
	;この告白内容表示後に接受かどうかの選択分が表示されます
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF FIRSTTIME("告白")
			PRINTFORML 「%CALLNAME:MASTER%……%CALLNAME:MASTER%—！」
			PRINTFORMW 「那个～……因为没办法再忍下去了，要说了哦」
			PRINTFORML 
			PRINTFORMW 「我，喜欢%CALLNAME:MASTER%」
			PRINTFORMW 「想要关系变得更～加更加好，希望能一直在一起……」
			PRINTFORMW 「嘛，那个～……就像是、恋人一样」
			PRINTFORMW 「就算只是在我的周围，也有很多其他可爱的女孩……我知道这可能很难但—」
			PRINTFORMW 「就算是这样……」
			PRINTFORML
			PRINTFORMW 「呐，%CALLNAME:MASTER%」
			PRINTFORMW 「可以只看着我一个人吗……？」
		ELSE
			PRINTFORML 「呐～、有没有后悔把我甩掉了呢？」
			PRINTFORMW 「现在的话依然还是OK的哦，所以……」
			PRINTFORMW 「怎么样？」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白オッケー（恋人が付く）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
			PRINTFORML 「太好啦—%UNICODE(0x2665) *1%　谢谢，%CALLNAME:MASTER%%UNICODE(0x2665) *1%」
			PRINTFORML 「之后再对其他人说“可以”什么的，可是不行了的哦！」
			PRINTFORMW 「放心，不会让你感到无聊的呢%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
			PRINTFORML 「嗯～呜、不行？　为什么啊～」
			PRINTFORMW 「呐、到底是喜欢什么样的呢？」
			PRINTFORMW 「这种程度我可是不会放弃的啊！」
		RETURN 1
	ENDIF

ENDIF
RETURN 1

;==================================================
;约会帰り
;==================================================
@M_KOJO_SPEVENT_K104_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_3(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF CFLAG:318 == 1
			PRINTFORML 
		ELSEIF TCVAR:烂醉 == 1
			PRINTFORML 
		ELSEIF TALENT:恋慕 || TALENT:愛欲
			PRINTFORMW 「今天过得好開心啊！　下一次也很期待的哦」
			PRINTFORMW 「……那，之后要做什么呢？……%UNICODE(0x2665) *1%」
		ELSEIF TALENT:思慕
			PRINTFORML 「和%CALLNAME:MASTER%在一起，我还挺喜欢的哦」
			PRINTFORMW 「下次，再带上我去哪里转转吧%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORML 「果然，魔界外面也相当新鲜有趣的啊！」
			PRINTFORMW 「其他的也是，如果还知道有什么好地方要告诉我呢」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML 「ん～？　わたし、別にもう怒ってないよっ」
		PRINTFORMW 「ちゃんと反省したなら、許してあげようじゃない」
		PRINTFORML 
		PRINTFORMW 「……反省、してるよね？」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML 「わかった、わかったわ……稍微だけ許してあげる」
		PRINTFORMW 「稍微だけだからね？」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「仲直りできると思ってたんだ」
		PRINTFORML 「まぁね～、わたしも他人と険悪なのは疲れるし」
		PRINTFORMW 「誠意に免じて、3割ほどは憤怒を和らげてあげようじゃない」
		PRINTFORMW 「でも、まだまだ怒ってるからね！　7割くらい」
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

