			TALENT:127:81 = 0
			CALL COLORMESSAGE(@"瓔花は【不信任人類】を失った",C_YELLOW,1)
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML 「しくしくしく…告白が失敗するなんて…」
		PRINTFORMW 「しょうがない。また一から積み上げていくしかないよね」
		PRINTFORMW 「%CALLNAME:MASTER%……これからも宜しくね」
		RETURN 1
	ENDIF
ENDIF
RETURN 1


