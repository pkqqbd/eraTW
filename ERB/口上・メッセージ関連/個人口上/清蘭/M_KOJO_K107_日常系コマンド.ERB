TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K107_603
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K107_603_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K107_603_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;憤怒
	IF CFLAG:318 == 1
		PRINTFORML 「……なにしてんの？」
		PRINTFORMDW 清蘭に冷目で睨まれた…
	ELSEIF TALENT:恋人
		SELECTCASE RAND:4
			CASE 0
				CALL HPH_PRINT,@" 「ん……HPH」","L"
				PRINTFORMDW どちらからということもなく、自然に清蘭と手をつないだ。
			CASE 1
				PRINTFORMDL 清蘭とお互相用指を絡ませつつ手をつないだ。所謂、"恋人つなぎ"だ。
				CALL HPH_PRINT,@" 「えへへHPH …稍微歩きにくいね…HPH」","W"
			CASE 2
				CALL HPH_PRINT,@" 「…そっか。私たち、もう恋人だもんねHPH」","L"
				PRINTFORMDW 清蘭は少し恥ずかしがりながらも、しっかりと手を握り返してくれた。
				CALL HPH_PRINT,@" 「私の事、絶対離さないでね…HPH」","W"
			CASE 3
				CALL HPH_PRINT,@" 「……あっHPH　えへへ…HPH」","L"
				PRINTFORMDW 手をつなぐと、清蘭は開心地顔を綻ばせた。
		ENDSELECT
	ELSEIF TALENT:思慕 || TALENT:恋慕
		SELECTCASE RAND:4
			CASE 0
				PRINTFORML 「…………♪」
				PRINTFORMDW 清蘭と手をつなぐと、何も言わず握り返してきた。
			CASE 1
				PRINTFORML 「%CALLNAME:MASTER%。手、出して」
				PRINTFORMDW 言われるがままに手を出すと、清蘭は%CALLNAME:MASTER%の手をギュっと握った。
				PRINTFORMW 「ほ～ら！早く行こ♪」
				PRINTFORMDW …清蘭の笑顔に、少しだけドキドキした。
			CASE 2
				PRINTFORML 「…ねえ%CALLNAME:MASTER%。何か忘れてない？」
				PRINTFORMDW 忘れる筈がないだろう、と清蘭の手をしっかりと握った。
				PRINTFORMW 「是、よろしい♪」
			CASE 3
				PRINTFORML 「急に手をつなごう、だなんて…なんでよ？」
				PRINTFORMW 「…はぐれないために手をつなぐ？…まあ、そういうことなら…うん…」
				PRINTFORMDW 目的地まで清蘭と手をつないで行くことにした。
		ENDSELECT
	;それ以外
	ELSE
		SELECTCASE RAND:4
			CASE 0
				PRINTFORML 「は…？はあっ！？何で恋人でもない你と手なんかつなぐのよ！？」
				PRINTFORML 「嫌かって…えっと、嫌…じゃないけどその…恥ずかしい…というか…」
				PRINTFORMW 「ッ！？～～～！！」
				PRINTFORMDW 煮え切らない態度の清蘭と、少し強引に手をつないだ。…存外嫌なわけでもなさそうだ。
			CASE 1
				PRINTFORML 「へっ！？手を…？だって…恥ずかしいし…」
				PRINTFORML 「う…わ、わかったわよ！つなげばいいんでしょ！？」
				PRINTFORMW 「是っ！！ぎゅう～～～～！！」
				PRINTFORMDW 照れ隠しだろうか、清蘭は強い力で握ってくる。手が痺れて感覚が無くなってきた…
			CASE 2
				PRINTFORML （…今日は手、つながないのかな…）
				PRINTFORMW 「あっ…！……～～！！」
				PRINTFORMDW …坦率じゃない清蘭の"期待"に応え、手をつないだ。
			CASE 3
				PRINTFORML 「……是、どーぞ」
				PRINTFORMDW ……？ずい、と出してきた清蘭の手を凝視するも、何も見つからない――
				PRINTFORMW 「…あーもー！手つなぐんでしょ！恥ずかしいんだからほら！早く！！」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
