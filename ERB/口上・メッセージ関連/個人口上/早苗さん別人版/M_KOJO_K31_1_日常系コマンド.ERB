@M_KOJO_早苗追加_MESSAGE_COM_K31_300
;指令共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～说着话。」とかが消えます。また、削除した場合は自動で口上色が付きません。
CALL TRAIN_MESSAGE
;口上メイン呼出
CALL M_KOJO_早苗追加_MESSAGE_COM_K31_300_1
RETURN RESULT

@M_KOJO_早苗追加_MESSAGE_COM_K31_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;ここに口上書いて下さい

	;デ背包用
	;PRINTFORML 「現在ストーリー_第{CFLAG:31:1001}章_第{CFLAG:31:1002}節」
	;CALL K31_特殊会話フラグの確認と出力("デ背包")
	;PRINTFORML 「現在ストーリー_第{CFLAG:31:1001}章_第{CFLAG:31:1002}節」	
	
	;=======================
	;憤怒、失敗、烂醉分岐を先に書いておく、RETURNさせる
	;=======================
	;憤怒
	IF CFLAG:ブチギレ
		PRINTFORML 「…………」
		PRINTFORMDW %CALLNAME:TARGET%は怒っているようだ……
		RETURN 1
	;失敗
	ELSEIF TFLAG:193 == -1
		IF TALENT:恋人 || TALENT:恋慕
			PRINTFORML 「……なんですっ？」
			PRINTFORMW 「ごめんなさい、ぼーっとしてました」
		ELSE
			PRINTFORML 「……………あーっと……」
			PRINTFORMW 「話題がつきました……」
		ENDIF
		RETURN 1
	;烂醉
	ELSEIF TCVAR:烂醉
		PRINTFORMDL %CALLNAME:TARGET%は酔いが回りすぎたのか、気分が悪そうだ…
		PRINTFORMW 「うぅ… きもぢわるぃです…」
		RETURN 1
	ENDIF
	
	;===================================================================
	;ストーリーイベント発生判定、詳細はユーザー関数.ERBを参照
	;===================================================================
	CALL K31_特殊会話フラグの確認と出力("会話")
	SIF RESULT
		RETURN 1
	
	;==========================================================================================
	;1/3で日期会話発生、確率ちょい高めに、通常会話は発生させない（RETURN消して発生させることもできる）
	;==========================================================================================
	IF !RAND(2)
		SELECTCASE DAY:2
			CASE 1 
			;元旦
				IF DAY:3 == 1
					PRINTFORML 「あけまして祝贺您！」
					PRINTFORMW 「今年もよろしくお願いしますね」
					RETURN 1 
				ENDIF
			;白色情人節
				IF DAY:3 == 14
					PRINTFORML 「今日は白色情人節ですね」
					PRINTFORMW 「だから何ってわけでもないですけど」
					RETURN 1 
				ENDIF
			;例大祭
				IF DAY:3 == 25
					PRINTFORML 「今日は例大祭です」
					PRINTFORMW 「これを機に信者が増えてくれればいいんですが」
					RETURN 1 
				ENDIF
			CASE 2
			;七夕
				IF DAY:3 == 7
					PRINTFORML 「今日は七夕ですね」
					PRINTFORMW 「短冊には何を書きますか？」
					RETURN 1 
				ENDIF
			CASE 3
			;お赏月
				IF NAME_MOON(DAY:3) == "十五夜 -中秋的名月- "
					PRINTFORML 「今日は十五夜です」
					PRINTFORMW 「お赏月でもしましょうか」
					RETURN 1 
				ENDIF
			;風神「二百十日」
				IF DAY:3 == 1
					PRINTFORML 「今日は立春から二百十日目」
					PRINTFORMW 「台風など天気が荒れやすいと言われている日ですね」
					RETURN 1 
				ENDIF
			;萬聖節
				IF DAY:3 == 30
					PRINTFORML 「今日は萬聖節です」
					PRINTFORMW 「お菓子かいたずらか、どっちがいいです？」
					RETURN 1 
				ENDIF
			CASE 4
			;情人節
				IF DAY:3 == 14
					PRINTFORML 「今日はバレンタインです」
					PRINTFORMW 「せっかくですし、巧克力食べますか」
					RETURN 1 
				ENDIF
			;節分
				IF DAY:3 == 3
					PRINTFORML 「今日は節分です」
					PRINTFORMW 「おにはーそと、ふくはーうち、って奴ですね」
					RETURN 1 
				ENDIF
			;聖誕節
				IF DAY:3 == 25
					PRINTFORML 「今日は聖誕節です」
					PRINTFORMW 「蛋糕、食べたいですね」
					RETURN 1 
				ENDIF
			;大晦日
				IF DAY:3 == 31
					PRINTFORML 「今日は大晦日です」
					PRINTFORMW 「一年の締めくくりですね」
					RETURN 1 
				ENDIF
		ENDSELECT
	ENDIF
	;=======================
	;ここから通常会話
	;=======================
	IF TALENT:恋人 || TALENT:恋慕
		LOCAL = 10
	ELSEIF TALENT:思慕
		LOCAL = 7
	ELSE
		LOCAL = 5
	ENDIF
	
	SELECTCASE RAND(LOCAL)
		CASE 0
			PRINTFORML 「どうやって飛んでいるのか、ですか？」
			PRINTFORML 「まぁ…奇跡の力とでも思ってください」
		CASE 1
			PRINTFORML 「幻想郷は宴会が多いだけあって酒強い人が多いですけど、私は実は酒苦手なんです」
			PRINTFORML 「すぐ酔いつぶれない上戸の方がうらやましくなるときもあります」
			PRINTFORML 「そういえば大喝酒の表現っていろいろありますけど、%CALLNAME:MASTER%さんはどの表現を使いますか？」
			PRINTFORML 「上戸？海量？ザル？　左党やワクといった言葉もあるみたいですが」
		CASE 2
			PRINTFORML 「等身大の巨大ロボとか見てみたいです」
			PRINTFORML 「え？外の世界にはあるんですか？早く幻想入りしてほしいですね…」
		CASE 3
			PRINTFORML 「能力で天候操作をしてほしい？あまり大規模だと詠唱とか大変なんですよね…」
			PRINTFORML 「まあ、機会があったら考えときます」
		CASE 4
			PRINTFORML 「信仰は、人类にとって非常に大切なものです」
			PRINTFORML 「信仰が無ければ神の力は弱まり、人に恩恵を与えることも難しくなってしまいます」
	 ;ここから思慕以上で表示
		CASE 5
			PRINTFORML 「引っ越してきたとき是ろいろと不安もありましたが、こちらの生活も悪くないものです」
			PRINTFORML 「楽しい話相手も居ますし、ね？」
		CASE 6
			PRINTFORML 「%CALLNAME:MASTER%さんもそうですけど、私の知ってる人类って妖怪じみた方ばっかりなんです…」
			PRINTFORML 「そのうち自分もそんな風になっちゃうんですかね…」
			PRINTFORMDL もうなってるのでは？という言葉を%CALLNAME:MASTER%はすんでのところで飲み込んだ…
		CASE 7
			PRINTFORML 「%CALLNAME:MASTER%さんって会話の引き出し多いですよね」
			PRINTFORML 「秘訣とか教えてもらえませんか？信仰集めにはやっぱり話術も大切ですし」
	;ここから恋慕以上で表示
		CASE 8
			PRINTFORML 「ふふ………%CALLNAME:MASTER%さんとの会話は楽しくて好きです」
			PRINTFORML 「もちろん、%CALLNAME:MASTER%さんのことも……」
		CASE 9
			PRINTFORML 「最近、何かに夢中で時間を忘れてしまうことがあるんです」
			PRINTFORML 「%CALLNAME:MASTER%さんといるときは、特に」
	;ねんのため
		CASEELSE
			PRINTFORML 「＊＊＊＊会話口上において予期しない分岐先に到達しました＊＊＊＊」
	ENDSELECT	
	

	PRINTFORMW
ENDIF
RETURN 1
