;1,六九式
;==================================================
@M_KOJO_MESSAGE_SCOM_K134_1
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K134_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K134_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 「让我们来比比看谁先高潮吧%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		;精液中毒4以上
		IF ABL:精液中毒 > 4
			IF RAND:3 == 0
				PRINTFORMW 「嗯？嗯嗯嗯～%UNICODE(0x2665) *1%」
				PRINTFORMW 美宵感觉到%CALLNAME:MASTER%即将要射精、用舌头巧妙地催促着。
				PRINTFORMW （再过一会儿就会有很多美味的新鲜精液出来了呢……%UNICODE(0x2665) *1%）
			ELSEIF RAND:2 == 0
				PRINTFORMW 「嗯%UNICODE(0x2665) *1%嗯啾%UNICODE(0x2665) *1%」
				PRINTFORMW 「很多无法忍耐的我慢汁流出来了呢、请在美宵的嘴里射精吧%UNICODE(0x2665) *1%」
				PRINTFORMW 美宵感觉到%CALLNAME:MASTER%即将要射精、用舌头巧妙地催促着。
			ELSE
				PRINTFORMW 「嗯嗯%UNICODE(0x2665) *1%肉棒越来越硬了呢%UNICODE(0x2665) *1%」
				PRINTFORMW 美宵感觉到%CALLNAME:MASTER%即将要射精、用舌头巧妙地催促着。
				PRINTFORMW （好想快点喝浓厚的精液啊%UNICODE(0x2665) *1%）
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「嗯嗯%UNICODE(0x2665) *1%」
				PRINTFORMW 美宵感觉到%CALLNAME:MASTER%即将要射精、用舌头巧妙地催促着。
				PRINTFORMW 「肉棒越来越硬了、看来快要射精了呢%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「嗯嗯%UNICODE(0x2665) *1%嗯啾%UNICODE(0x2665) *1%」
				PRINTFORMW 美宵感觉到%CALLNAME:MASTER%即将要射精、用舌头巧妙地催促着。
			ELSE
				PRINTFORMW 「嗯嗯%UNICODE(0x2665) *1%已经快要射精了吗%UNICODE(0x2665) *1%」
				PRINTFORMW 美宵感觉到%CALLNAME:MASTER%即将要射精、用舌头巧妙地催促着。
			ENDIF
		ENDIF
	;舌3以上
	ELSEIF ABL:舌 > 2
		;精液中毒4以上
		IF ABL:精液中毒 > 4
			IF RAND:3 == 0
				PRINTFORMW 「嗯嗯%UNICODE(0x2665) *1%嗯嗯嗯%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「嗯嗯%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
				PRINTFORMW 「肉棒又大又硬的、好难含住%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「啾%UNICODE(0x2665) *1%哈呼%UNICODE(0x2665) *1%」
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「嗯～%UNICODE(0x2665) *1%」
				PRINTFORMW 「美宵的嘴舒服吗%UNICODE(0x2665) *1%我会让您更舒服的哦%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「嗯嗯%UNICODE(0x2665) *1%请放心交给美宵吧%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「嗯嗯啾%UNICODE(0x2665) *1%我会让肉棒更加舒服的%UNICODE(0x2665) *1%」
			ENDIF
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「嗯%UNICODE(0x2665) *1%啾啾%UNICODE(0x2665) *1%」
			PRINTFORMW 「肉棒一抖一抖的、好可爱啊%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯啾%UNICODE(0x2665) *1%嗯嗯%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「嗯嗯……%UNICODE(0x2665) *1%」
			PRINTFORMW 美宵有些不自然地含住肉棒。
		ENDIF
	ENDIF
ENDIF
