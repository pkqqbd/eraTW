@M_KOJO_MESSAGE_COM_K134_19
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K134_19_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K134_19_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;これ継続接吻だから初めてはおかしいと思う。一応通常接吻のコピペ。
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「啾%UNICODE(0x2665) *1%」
		PRINTFORMW 「……与美宵的接吻、请永远不要忘记哦……%UNICODE(0x2665) *1%」
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「嗯%UNICODE(0x2665) *1%」
			PRINTFORMW 「更多……请再多给我一点%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「啾%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「嗯%UNICODE(0x2665) *1%」
			PRINTFORMW 「光接吻是不够的哦%UNICODE(0x2665) *1%」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「啾%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「呼啊%UNICODE(0x2665) *1%更多……%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「哈啊啊……%UNICODE(0x2665) *1%」
		ENDIF
	ENDIF
ENDIF
RETURN 1

