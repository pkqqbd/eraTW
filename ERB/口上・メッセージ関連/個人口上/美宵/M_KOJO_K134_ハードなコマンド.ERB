;==================================================
;150,雙乳間抽插
;==================================================
@M_KOJO_MESSAGE_COM_K134_150
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K134_150_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K134_150_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「呜啊啊、就这样突然插进来是不对的哦%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「请在乳穴里满满地射出来吧%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「乳交的时候会很兴奋吗%UNICODE(0x2665) *1%肉棒越来越大了呢%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「哎呀！太粗暴了%UNICODE(0x2665) *1%」
		ENDIF
	;胸3以上
	ELSEIF ABL:胸 > 2
		IF RAND:3 == 0
			PRINTFORMW 「请用美宵的胸部舒服起来吧%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯%UNICODE(0x2665) *1%用胸来做舒服吗%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「请多多射出来吧%UNICODE(0x2665) *1%」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「嗯嗯%UNICODE(0x2665) *1%感觉很舒服呢%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「请多多侵犯胸部吧%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「请不要对胸部太粗暴了啊%UNICODE(0x2665) *1%」
		ENDIF
	ENDIF
ENDIF
RETURN 1
