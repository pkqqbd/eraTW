;==================================================
;140,深喉插入
;==================================================
@M_KOJO_MESSAGE_COM_K134_140
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K134_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K134_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「嗯！哈啊啊～～～！」
		RETURN 1
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「呵呵%UNICODE(0x2665) *1%」
			PRINTFORMW 美宵因为被%CALLNAME:MASTER%的肉棒深深插入而呻吟着。
		ELSEIF RAND:2 == 0
			PRINTFORMW 「啊%UNICODE(0x2665) *1%嗯%UNICODE(0x2665) *1%」
			PRINTFORMW 美宵因为被%CALLNAME:MASTER%的肉棒深深插入而呻吟着。
		ELSE
			PRINTFORMW 「嗯嗯%UNICODE(0x2665) *1%呀%UNICODE(0x2665) *1%」
			PRINTFORMW 美宵因为被%CALLNAME:MASTER%的肉棒深深插入而呻吟着。
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「嗯嗯～！」
			PRINTFORMW 美宵因为被%CALLNAME:MASTER%的肉棒深深插入而呻吟着。
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯啊啊～～～！！！」
			PRINTFORMW 美宵因为被%CALLNAME:MASTER%的肉棒深深插入而呻吟着。
		ELSE
			PRINTFORMW 「啊啊！！！」
			PRINTFORMW 美宵因为被%CALLNAME:MASTER%的肉棒深深插入而呻吟着。
		ENDIF
	ENDIF
ENDIF
RETURN 1

