;626,土産物屋
;TFLAG:194(-2=ひやかし -1=見送り 1=BAD 2=NOMAL 3=GOOD)
;==================================================
@M_KOJO_MESSAGE_COM_K134_626
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K134_626_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K134_626_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
	IF LOCAL
	SELECTCASE TFLAG:194
	;ひやかし
	CASE -2
		IF RAND:3 == 0
			PRINTFORMW 「冷やかしだと思われたみたいね……」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「ああ……出禁になっちゃう……」
		ELSE
			PRINTFORMW 「鯢呑亭の看板娘が怒られるなんて責任取ってくださいね？」
		ENDIF
	;果然還是放棄（見送り）
	CASE -1
		IF RAND:3 == 0
			PRINTFORMW 「んーめぼしい物は無かったわね」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「鯢呑亭には似合わないものばかりね」
		ELSE
			PRINTFORMW 「また今度来ましょうね、次は何か那个ばいいなぁ」
		ENDIF
	;400点未満（BAD）
	CASE 1
		IF RAND:3 == 0
			PRINTFORMW 「ええ……受け取りますが……」
			PRINTFORMW 美宵はあまり嬉しくなさそうだ……
		ELSEIF RAND:2 == 0
			PRINTFORMW 「……お店に置いときますね」
			PRINTFORMW 美宵はあまり嬉しくなさそうだ……
		ELSE
			PRINTFORMW 「うーん……一応頂くわね」
			PRINTFORMW 美宵はあまり嬉しくなさそうだ……
		ENDIF
	;400点～699点（NOMAL）
	CASE 2
		IF RAND:3 == 0
			PRINTFORMW 「美宵にくれるんですか？ありがとうございます！」
			PRINTFORMW 美宵は少しはにかんだ笑顔を浮かべた。
		ELSEIF RAND:2 == 0
			PRINTFORMW 「わあっ！嬉しいわ！」
			PRINTFORMW 美宵は少しはにかんだ笑顔を浮かべた。
		ELSE
			PRINTFORMW 「禮物？ありがとうね%UNICODE(0x2665) *1%」
			PRINTFORMW 美宵は少しはにかんだ笑顔を浮かべた。
		ENDIF
	;700点以上（GOOD）
	CASE 3
		IF RAND:3 == 0
			PRINTFORMW 「ふふっ美宵の欲しいものが分かるなんて流石%CALLNAME:MASTER%さんね%UNICODE(0x2665) *1%」
			PRINTFORMW 美宵は臉上泛起了紅潮開心地している。
		ELSEIF RAND:2 == 0
			PRINTFORMW 「わあ！嬉しいわ！お客さんに自慢しちゃおうっと%UNICODE(0x2665) *1%」
			PRINTFORMW 美宵は臉上泛起了紅潮開心地している。
		ELSE
			PRINTFORMW 「これ欲しかったの！大事にするわ！」
			PRINTFORMW 美宵は受け取った物抱住、臉上泛起了紅潮開心地している。
		ENDIF
	ENDSELECT
ENDIF
RETURN 1
