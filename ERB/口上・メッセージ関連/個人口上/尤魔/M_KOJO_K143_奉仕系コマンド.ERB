;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_KX_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_KX_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_KX_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;初めての阴茎
		IF CFLAG:1003 == 0
			SELECTCASE TALENT:MASTER:形状
			CASE 6,10
				PRINTFORML 「！！！」
				PRINTFORML 「は、なんだぁ、こりゃあ…」
				PRINTFORML 「私の腕より太くないか？　これ」
				IF BASE:MASTER:勃起 >= 1000
					PRINTFORML 「血管も浮き出て…こいつは食いごたえがありそうだ」
					PRINTFORML 「クックック、冗談だよ、冗談。こいつをしごけばいいんだな？」
				ELSE
					PRINTFORML 「硬くなってないのに、こんな大きさをしてるのか…」
					PRINTFORML 「これが、もっと大きくなるんだろう？　ぜひ見てみたいな」
				ENDIF
			CASE 1,2
				PRINTFORML 「ほうほう、随分と可愛的ものだ」
				PRINTFORML 「なぁに、恥ずかしがることはない。私の体にはちょうどいいくらいだろう」
				PRINTFORML 「どれ、まずは扱いてやろうじゃあないか」
			CASE 5
				PRINTFORML 「クックック、随分と凶悪的面構えをしている」
				PRINTFORML 「これで女たちを泣かせてきたっていうのか。そして、私のこともそうしようっていうんだろう？」
				IF TALENT:MASTER:非童貞 == 0
					PRINTFORML 「何…一度もそういう経験がないだと…？」
					PRINTFORML 「……」
					PRINTFORML 「なんだ…その…悪かったな」
				ELSE
					PRINTFORML 「楽しみだな。だがまずは前戯としゃれこもうじゃないか」
					PRINTFORML 「……」
					PRINTFORML 「しかしこれ、何か握りづらいな…」
				ENDIF
			CASE 4
				PRINTFORML 「…なあ、おい」
				PRINTFORML 「この眩しいのどうにかならんか？」
				PRINTFORML 「気が散ってやる気がおきんのだが…」
			CASEELSE
				PRINTFORML 「ほう…これがお前の阴茎というやつか」
				PRINTFORML 「見た目はどこかソーセージで美味そうだなぁ…食ったらダメか？」
				PRINTFORML 「クックック、冗談だよ、冗談」
				PRINTL 
			ENDSELECT
		;阴茎2回目以降
		ELSE
			SELECTCASE TALENT:MASTER:形状
			CASE 6,10
				PRINTFORML 「相変わらず、なんてデカさだ…」
				IF TALENT:130:恋人
					PRINTFORML 「このぶっといもので、吉弔のをヨガらせてるのか？」
				ELSEIF TALENT:133:恋人
					PRINTFORML 「いかにも驪駒のが好みそうな大きさだ」
				ENDIF
				IF BASE:MASTER:勃起 >= 1000
					PRINTFORML 「血管も浮き出て…こいつは食いごたえがありそうだ」
					PRINTFORML 「クックック、冗談だよ、冗談。ほら、シゴいてやるからじっとしてろ」
				ELSE
					PRINTFORML 「これがさらにデカくなるっていうんだからなぁ…驚きだよ」
					PRINTFORML 「ほれ、さっさと勃たせてみろ」
				ENDIF
				PRINTL 
			CASE 1,2
				PRINTFORML 「クックック、相変わらず可愛的見た目をしているなあ」
				IF TALENT:130:恋人
					PRINTFORML 「吉弔のはこういうのが好みなのか？」
				ELSEIF TALENT:133:恋人
					PRINTFORML 「こんなに小さくても驪駒を満足させているのか？　大したもんだな」
				ENDIF
				PRINTFORML 「さてさて、硬さと持続力はどんなもんかな…？」
				PRINTL 
			CASE 5
				PRINTFORML 「クックック、相変わらず凶悪的面構えをしている」
				IF TALENT:130:恋人
					PRINTFORML 「こんなブツを突っ込まれてよがるとは、吉弔のも趣味がいいんだか悪いんだか」
				ELSEIF TALENT:133:恋人
					PRINTFORML 「驪駒のは、普通のモノじゃ満足できないのか？」
				ENDIF
				PRINTL 
			CASE 4
				PRINTFORML 「…相変わらず眩しいな」
				PRINTFORML 「やりづらくってかなわん」
				PRINTL 
			CASEELSE
				PRINTFORML 「クックック、相変わらずうまそうな見た目だなぁ」
				PRINTFORML 「先っぽだけでも食ったらダメか？　いいじゃないか、先っぽくらい」
				PRINTL 
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

