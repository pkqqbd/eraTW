;TFLAG:194(1000以上ならTARGETが釣った)
;==================================================
@M_KOJO_MESSAGE_COM_K143_621
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K143_621_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K143_621_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;你が釣った
	IF TFLAG:194 < 1000
		SELECTCASE TFLAG:193
		;釣れない
		CASE 0
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「すまんな。私は馬鹿だからお前が見せている大物が見えんのだ」
			CASE 1
				PRINTFORML 「くくっ。きっと大物だったに違いない」
				PRINTFORML 「よく言うだろう？　『逃がした魚は大きい』って」
			ENDSELECt
		;魚
		CASE 1 TO 4
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「ほお、やるじゃないか」
			CASE 1
				PRINTFORML 「この私を差し置いて先に釣魚上げるとはな」
			CASE 2
				PRINTFORML 「ほほう、見事じゃないか。」
				IF K143C_剛欲同盟 == 1
					PRINTFORML 「お前をうちの釣魚部隊長に任命してやろう。光栄に思え」
				ELSE
					PRINTFORML 「お前、うちの専属釣魚師にならないか？　好待遇を約束するぞ」
				ENDIF
			ENDSELECT
		;魚
		CASE 11 TO 15
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「……」
				PRINTFORML 尤魔は羨ましそうに%CALLNAME:MASTER%が釣った魚を見ている
			CASE 1
				PRINTFORML 「なあ、%CALLNAME:MASTER%」
				PRINTFORML 「お前とは、いい付き合いをしているよな」
				PRINTFORML 「今後ともいい付き合いをしていくために、その魚を私に譲ろうとは思わんか？　ん？」
			CASE 2
				PRINTFORML 「ほう、すごく美味そうな魚じゃないか」
				PRINTFORML 「私の腹が魚の気分になった。責任をとってそれで何か美味いものを作れ」
			ENDSELECT
		;跳蛋
		CASE 21
			IF K143C_性に貪欲 > 0 && K143C_性に貪欲 != 2
				PRINTFORML 「…いや、使わんが」
				PRINTFORML 「いくら私がエッチなことが好きでも、川で拾ったやつなんて御免だぞ」
			ELSE
				PRINTFORML 「こんなもの川に捨てる奴がいるのか…」
				PRINTFORML 「もしくは、川に棲む妖怪が愛用していものをうっかり流してしまったかだな…」
			ENDIF
		;守矢之守札
		CASE 131
			PRINTFORML 「こんなところで拾うとはな。案外ご利益がすごいかもしれんぞ」
			PRINTFORML 言った本人は欠片ほども思っていなそうだ
		;龍之爪
		CASE 606
			PRINTFORML 「龍之爪か…本当に存在していたとはな」
			PRINTFORML 「貴重な品だ。無駄遣いするなよ。薬にも武具にもなる」
			PRINTFORML 「一番いいのは、私に預けることだ。くっくっく、冗談だよ」
			PRINTW 
		;氷之鱗
		CASE 609
			PRINTFORML 「ほう、綺麗なもんだ」
			PRINTFORML 「何かに使えないか」
		;木材
		CASE 610
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「くっくっく。大物じゃあないか」
			CASE 1
				PRINTFORML 「魚の形をした木材…？　悪戯にしちゃあ手が込んでるな」
			CASE 2
				PRINTFORML 「熊…？　の置物だなこれは。口にシャケをくわちゃあいるが」
			ENDSELECT
		;龍涎香
		CASE 643
			PRINTFORML 「ほう、甜香りがするな」
			PRINTFORML 「好事家なんかに売れそうな気がするな」
		;青蛙
		CASE 706
			PRINTFORML 「青蛙か」
			PRINTFORML 「間違えても生で食うなよ。私は平気だがな」
			PRINTW 
		ENDSELECT
	;尤魔が釣った
	ELSE
		SELECTCASE TFLAG:193
		;釣れない
		CASE 0
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「たっ、たまたまだ、たまたま。今日は調子が悪いんだ」
				PRINTFORML こちらから何も言っていないのに、尤魔が勝手に弁解を始めた
			CASE 1
				PRINTFORML 「…なんだ、言いたいことがあるなら言え」
				PRINTFORML 尤魔がギロリと睨んできた
			ENDSELECt
		;魚
		CASE 1 TO 4
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「まずまずってところか」
			CASE 1
				PRINTFORML 「どうだ、私の釣魚の腕は」
			CASE 2
				PRINTFORML 「くくくっ。今日の夕飯の当てができたな」
			ENDSELECT
		;魚
		CASE 11 TO 15
			SELECTCASE RAND:4
			CASE 0
				PRINTFORML 「くっくっく。今日は入れ食いだなあ」
				PRINTFORML 大物が連れて、尤魔は心情愉快だ
			CASE 1
				PRINTFORM 「こいつは美味そうだ。帰ったら同盟の奴らに食わせてやるとしよう」
			CASE 2
				PRINTFORML 「くくくっ。どうだ、見ろ。こいつは間違いなく美味いぞぉ」
				PRINTFORML 「私が調理すれば、こいつはもっと美味くなる。どうだ、食いたくなってきたか？」
			CASE 3
				PRINTFORML 「くくくっ。どうだ、見ろ。いい魚だろう」
				PRINTFORML 美味い魚をゲットし、尤魔はニコニコと心情愉快で笑着いる
			ENDSELECT
		;跳蛋
		CASE 21
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「…なんでこんなもんが川で釣れるんだ？」
			CASE 1
				PRINTFORML 「ひょっとして河童ども、水中でオナってやがるのか？」
			CASE 2
				PRINTFORML 「…なんだよ。私は使わんぞ」
			ENDSELECT
		;守矢之守札
		CASE 131
			PRINTFORML 「なんだこれは。符箓？」
			PRINTFORML 「ボロボロじゃないか。逆に呪われそうだな」
			PRINTW 
		;龍之爪
		CASE 606
			PRINTFORML 「え、なーにこれー。爪？」
			PRINTFORML 「これだけ大きいってことは、龍之爪か何か？　レアじゃん！」
			PRINTW 
		;氷之鱗
		CASE 609
			PRINTFORML 「綺麗ー。向こう側が透けて見える」
			PRINTFORML 「これ溶けたりしないのかな。念のため冷ところに保管しといたら？」
		;木材
		CASE 610
			SELECTCASE RAND:3
			CASE 0
				IF K143C_鬼傑組 == 1
					PRINTFORML 「…笑着んじゃねーよ。親に似て性格悪いな、お前」
				ELSEIF K143C_勁牙組 == 1
					PRINTFORML 「お前、自分のところの親じゃないからって遠慮なく笑いやがって…」
				ELSEIF K143C_剛欲同盟 == 1
					PRINTFORML 「くそっ…お前、後で覚えとけよ」
					PRINTFORML 「賄い飯に嫌いなものばっかり入れてやるからな」
				ELSE
					PRINTFORML 「くそっ、当たりがデカいと思ったらっ」
					PRINTFORML 「うっ、うるさいっ。木材が釣れるなんて普通思わんだろうがっ」
				ENDIF
			CASE 1
				PRINTFORML 「はぁ？　なんで木材が釣れるんだよっ。くそっ、重てぇ」
				PRINTFORML 「おい、笑着ないで手伝え。ああ、もう」
			CASE 2
				PRINTFORML 「…つぶれた饅頭みたいな顔していないで、笑いたきゃ笑えよ」
			ENDSELECT
		;青蛙
		CASE 706
			PRINTFORML 「青蛙か。まあ、おかずにはなるか」
			PRINTFORML 「なんだよ。美味いんだぞ、青蛙。特に唐揚げがな」
			PRINTFORML 「まあ、青蛙だったらなんでも食えるってわけじゃないが…特にお前ら人类はな」
			PRINTW 
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
