;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"EVENT"系イベント
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K117_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
	;増やしたいかも
		IF (DAY:2 == 1 && DAY:3 == 1) || (DAY:2 == 2 && DAY:3 == 7) || (DAY:2 == 4 && DAY:3 == 14) 
			CALL SPECIALDAY_EVENT_K117
		ELSEIF TALENT:恋慕 || TALENT:思慕
			PRINTFORML 「啊啦、%CALLNAME:MASTER%。难道说是来见我的吗？」
		ELSE
			PRINTFORML 「找我有什么事吗？」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;増やしたいかも
		IF (DAY:2 == 1 && DAY:3 == 1) || (DAY:2 == 2 && DAY:3 == 7) || (DAY:2 == 4 && DAY:3 == 14) 
			CALL SPECIALDAY_EVENT_K117
		ELSEIF TALENT:恋慕 || TALENT:思慕
			PRINTFORML 「啊啦、%CALLNAME:MASTER%。见到你很開心呀」
		ELSE
			PRINTFORML 「你好呀、%CALLNAME:MASTER%」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「那个、%CALLNAME:MASTER%」
		PRINTFORMW 「我可以一起进去吗……？」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML
		PRINTFORML 「…………」
		PRINTFORMW 「非、非常抱歉！」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
	;口上ノートを見る限りMASTERは把柄を握られているそうなので
		PRINTFORML 「啊啦、这不是%CALLNAME:MASTER%嘛」
		PRINTFORMW 「我也想要洗澡呢」
		PRINTFORMW 「所以可以请你出去吗？」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		IF (DAY:2 == 1 && DAY:3 == 1) || (DAY:2 == 2 && DAY:3 == 7) || (DAY:2 == 4 && DAY:3 == 14) 
			CALL SPECIALDAY_EVENT_K117
		ELSE
			PRINTFORMW 「你好、%CALLNAME:MASTER%」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他角色と约会中に遭遇した
	;ARG:1に约会的对象
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		IF TALENT:恋慕
			PRINTFORML 「真嫉妒呢……」
			PRINTFORML 感觉听到了幻月的自言自语。
			PRINTFORMW 「什么也没有说噢」
		ELSE
			PRINTFORML 「啊啦、%CALLNAME:MASTER%」
			PRINTFORMW 「……看起来你很開心呢」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MAIN_MAP上で一日最初の挨拶
	;注、このあと通常の遭遇口上、ARG==1 OR ARG==2に接続します
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で一日最初の挨拶
	;ARG:1に约会的对象、ARG:1が0の場合は非约会
	;注、このあと通常の外出先で遭遇した口上、ARG==6 OR ARG==7に接続します
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		;ARG:1と约会中
		IF ARG:1
		;一人で外出
		ELSE
		ENDIF
		PRINTFORML 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;特別な日の遭遇時
;==================================================
@SPECIALDAY_EVENT_K117
;--------------------------------------------------
IF DAY:2 == 1
	;謹賀新年
	SELECTCASE DAY:3
		CASE 1
			PRINTFORML 「あけましておめでとう、%CALLNAME:MASTER%」
			PRINTFORMW 「今年も一年よろしくね」
	ENDSELECT
ELSEIF DAY:2 == 2
	;七夕
	SELECTCASE DAY:3
		CASE 7
    		PRINTFORML 「今日は織姫と彦星が年に一度だけ会える日だそうね」
			SELECTCASE CFLAG:好感度
				CASE 0 TO 1000
					PRINTFORML 「ロマンティックでいいと思うわ」
    				PRINTFORMW 幻月はあまり興味がなさそうだ。
				CASE 1000 TO 2000
				    PRINTFORMW 「人を愛するのは悪いことではないけど、仕事をさぼるのは、ねぇ」
				CASE IS > 2000
					PRINTFORML 「そういえば、%CALLNAME:MASTER%はもう短冊にお願い事は書いたかしら？」
					IF RAND:2 == 0
    					PRINTFORMW 「私？　世界平和を願っておいたわ」
					ELSE
						PRINTFORML 「え？　もうそうな年じゃない？　私から見たらまだまだ兒童よ」
    					PRINTFORMW 思わぬところで人と悪魔の価値観の違いを感じてしまった。
					ENDIF
			ENDSELECT
	ENDSELECT
ELSEIF DAY:2 == 4
	;バレンタイン
	SELECTCASE DAY:3
		CASE 14
			;黑雷神巧克力以外は地の文追加
			IF CFLAG:好感度 > 3000
				PRINTFORMW 少しそわそわした様子の幻月が%CALLNAME:MASTER%の前に一歩踏み出して来た……。
				PRINTFORML
			ENDIF
			SELECTCASE CFLAG:好感度
				;黑雷神巧克力
				CASE 1000 TO 3000
					IF TALENT:思慕 || TALENT:恋慕 || TALENT:愛欲
						PRINTFORML 「%CALLNAME:MASTER%、これあげるわ」
