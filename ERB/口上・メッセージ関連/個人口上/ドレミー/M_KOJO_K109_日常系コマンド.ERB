;416,演奏
;TFLAG:193(1=大成功 0=成功 -1=失敗)
;TFLAG:使用楽器(1=電子琴 2=鋼琴 3=吉他 4=小號 5=小提琴)
;==================================================
@M_KOJO_SUCCESS_COM_K109_416
;成否判定
;TFLAG:192(-2=指令終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K109_416
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K109_416_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K109_416_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;初回会話
	IF FIRSTTIME(SELECTCOM)
		PRINTFORML 「看不出来你还喜欢音乐呢」
		PRINTFORMW 「但是你弹乐器的样子有点不协调，好像变成了什么样子…」
	ELSEIF ABL:MASTER:音楽技能 >= 3
		IF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「变得相当熟练了呢。我觉得可以自称专业人士哦」
					PRINTFORMDW 哆来咪似乎很喜欢%CALLNAME:MASTER%的演奏
				CASE 1
					PRINTFORML 「听说在幻想乡的人間之里广场偶尔会有街头表演，你混进去也不会有违和感吧？」
					PRINTFORMDL 当%CALLNAME:MASTER%说下次试试的时候，哆来咪開心地拿出了喇叭
					PRINTFORMW 「我也想一起，现在就開始练习吧」
					EXP:109:演奏経験 += 1
				CASE 2
					PRINTFORML 「下次能试着边弹边唱吗？虽然是个人偏见、但%MASTERNAME:109%给人的印象就是很擅长唱歌」
					PRINTFORMDL 说到起唱歌更想集中精力演奏，哆来咪一瞬流露出遗憾，随后双手合十
					PRINTFORMW 「如果是那样的话，我来唱吧。实际上我也会一些……比如摇篮曲的话」
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「这不是很熟练了吗。应该有好好练习了吧？」
					PRINTFORMDW 哆来咪面带笑容地听着演奏
				CASE 1
					PRINTFORMDL 演奏结束后，哆来咪小声鼓掌
					PRINTFORMW 「如果有这个本领的话，即使在宴会上演奏也不会被丢石头吧」
				CASE 2
					PRINTFORML 「据说在外面的世界里穿着露出度很高的衣服演奏乐器的话可以拿到钱哦」
					PRINTFORMDW 想象着哆来咪穿着那样的服装演奏的样子，不由得把手伸向了钱包
			ENDSELECT
		ENDIF
	ELSE
		IF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「说实话不能说是擅长，只能说是有练习过的程度而已」
					PRINTFORMDW 哆来咪如实地传达了自己的感想
				CASE 1
					PRINTFORML 「呜—嗯，这种程度的话可能会被扔石头…」
					PRINTFORMDW 看来幻想乡的居民很严格呢……
				CASE 2
					PRINTFORML 「我也应该认真地练习乐器了…」
					PRINTFORMDL 到底是什么让你改变主意了呢。%CALLNAME:MASTER%问了问哆来咪
					PRINTFORMW 「因为如果我会演奏的话、不就能和%MASTERNAME:109%合奏了吗」
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMDL 在哆来咪的面前演奏
					PRINTFORMW 「%MASTERNAME:109%演奏乐器、感觉就像画一样」
				CASE 1
					PRINTFORML 「如果是小号的话我也能吹哦」
					PRINTFORMDW 和哆来咪稍微配合了一下
					EXP:109:演奏経験 += 1
				CASE 2
					PRINTFORML 「我觉得节奏很不错哦。请再让我听听」
					PRINTFORMDW 看来还没到外行的地步
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
