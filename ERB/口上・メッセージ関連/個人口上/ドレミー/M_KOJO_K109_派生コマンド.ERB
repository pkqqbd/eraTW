;==================================================
;19,授乳PLAY
;==================================================
@M_KOJO_MESSAGE_SCOM_K109_19
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K109_19_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K109_19_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORML 「授乳手交?啥啊这种罪孽深重的行为…」
		PRINTFORMW 「我知道了我知道了所以请不要这么死命求我啦」
		RETURN 1
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:恋慕 || TALENT:愛欲 || TALENT:炮友 || TALENT:風騷
		SELECTCASE RAND:5
			CASE 0
				CALL HPH_PRINT,@"「奶喂喂～？HPH…什么的」","L"
				PRINTFORMDL %CALLNAME:MASTER%枕在哆来咪的膝上，像婴儿一样吮吸着乳头
				PRINTFORMDW 和哆来咪欣慰的笑容相反，被她玩弄的肉棒勃起得快要裂開了
			CASE 1
				PRINTFORML 哆来咪一边给%CALLNAME:MASTER%哺乳，一边撸动着高耸的肉棒
				PRINTFORMW 哆来咪用充满母性的眼神注视着像幼子一样吮吸着乳头的%CALLNAME:MASTER%
			CASE 2
				PRINTFORML 「简直就是个还在吃奶的孩子啊…」
				PRINTFORMDW 代替回答吸着已经变硬勃起的乳头，哆来咪抱着%CALLNAME:MASTER%的头苦闷着。
			CASE 3
				PRINTFORMDL 在哆来咪巧妙的手淫技巧下，不由得把嘴从乳头上松開喘息了起来。
				PRINTFORML 「kora！小宝宝可是不会用那么舒服的声音喘息的哟」
				PRINTFORMDW 对不起呢，哆来咪妈妈
			CASE 4
				CALL HPH_PRINT,@"「是个大婴儿呢…嗯HPH」","L"
				PRINTFORMDW 用力吮吸着硬挺挺勃起的乳头，哆来咪不自觉的握紧肉棒，苦恼于乳头带来的快感之中。
		ENDSELECT
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「简直就是个还在吃奶的孩子啊…」
				PRINTFORMDW 代替回答吸着已经变硬勃起的乳头，哆来咪抱着%CALLNAME:MASTER%的头苦闷着。
			CASE 1
				PRINTFORMDL 在哆来咪巧妙的手淫技巧下，不由得把嘴从乳头上松開喘息了起来。
				PRINTFORML 「kora！小宝宝可是不会用那么舒服的声音喘息的哟」
				PRINTFORMDW 对不起呢，哆来咪妈妈
		ENDSELECT
	;それ以外
	ELSE
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「没想到%MASTERNAME:109%会提出这种要求呢…」
				PRINTFORMDL 被哆来咪温柔撸动着，%CALLNAME:MASTER%像幼儿退行了一样，心无旁骛地吸着乳头。
				PRINTFORMDW 无所事事的哆来咪摆弄着%CALLNAME:MASTER%那坚挺勃起的阴茎
			CASE 1
				PRINTFORML 虽然感受到哆来咪冷漠的视线，但并没有停止吮吸乳头的动作
				PRINTFORMW 不一会儿，哆来咪好像也放弃了，无可奈何地撸起了肉棒
		ENDSELECT
	ENDIF
ENDIF
