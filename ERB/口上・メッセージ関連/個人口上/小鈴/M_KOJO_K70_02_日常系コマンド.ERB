@M_KOJO_MESSAGE_COM_K70_300
;指令共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～说着话。」とかが消えます
;CALL TRAIN_MESSAGE
DRAWLINE
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K70_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K70_300_1
#DIM GUEST, 2
#DIMS GUESTS
#DIM POINT
#DIM 约会会話, 120
;日が変わってたら约会会話リセット
SIF !TCVAR:70:352
	VARSET 约会会話
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	RESETCOLOR
	;ゲスト角色判定
	CALL M_KOJO_K70_GUEST(0)
	GUEST = RESULT
	GUEST:1 = RESULT:1
	GUESTS = %RESULTS%
	;ここに口上書いて下さい
	IF ALCOHOL_FACE(70) == "酔酒" || ALCOHOL_FACE(70) == "不省人事"
		PRINTFORML %CALLNAME:PLAYER%试着和%CALLNAME:TARGET%搭话、但是%CALLNAME:TARGET%只是哼哼了两句、听不懂在说什么
	;会話不能の地の文は恋慕でない場合は省略できず、恋慕の時は表示されないので、ここではどちらの場合も同じように表示されるようにする
	ELSEIF TCVAR:70:会話不能
		IF TALENT:70:恋慕
			PRINTFORML 想跟%CALLNAME:TARGET%搭话却不知道该说什么
			PRINTFORMW 周围充满着尴尬的空气…
		ENDIF
	ELSEIF GUESTS != ""
		CLEARLINE 2
		PRINTFORML 三个人会話
		DRAWLINE
		;文章上だけであってソース等は変わりません
		PRINTFORML %CALLNAME:PLAYER%和%CALLNAME:TARGET%、%GUESTS%三个人交替着说话
	ELSE
		;约会会話
		IF TALENT:70:恋慕 && FLAG:约会的对象 == 70 && DATENAME_SPOT() != "道中" && !约会会話:(CFLAG:70:現在位置 / 10)
			CLEARLINE 2
			PRINTFORML 去%DATENAME_SPOT()%约会
			DRAWLINE
		ENDIF
		PRINTFORM %CALLNAME:PLAYER%和%CALLNAME:TARGET%
		IF TALENT:70:恋慕
			PRINTDATA
				DATAFORM 坐在我的身旁
				DATAFORM 虽然有点害羞
				DATAFORM 但依然在这和谐的氛围里
				DATAFORM 一边微笑
				DATAFORM 一边嬉闹着
				DATAFORM ……稍微有一点热了
			ENDDATA
		ELSE
			PRINTDATA
				DATAFORM 一边乏味
				DATAFORM 却又无忧无虑地
				DATAFORM 喝着酒
				DATAFORM 说着什么
				DATAFORM 希望世界
				DATAFORM 能够和平的
				DATAFORM 可笑的话
			ENDDATA
		ENDIF
		IF TFLAG:SELECTCOM的分岐 > 0
			PRINTFORML 聊得热火朝天
		ELSEIF TFLAG:SELECTCOM的分岐 < 0
			PRINTFORML 话聊到一半断掉了
		ELSE
			PRINTFORML 聊了天
		ENDIF
	ENDIF
	PRINTW 
	CALL M_KOJO_COLOR_K70
	;酔ってる
	IF ALCOHOL_FACE(70) == "酔酒" || ALCOHOL_FACE(70) == "不省人事"
		IF !RAND:2
			PRINTFORML 「啊~！”啊，真是太厉害了~ ~！」
			PRINTFORMW 「你好厉害啊~！哇~ ~ ~！」
		ELSE
			PRINTFORML 「你好啊~！！我的王子殿下！」
			PRINTFORMW 「開玩笑啦啊哈哈啊哈哈哈~！！」
		ENDIF
		PRINTFORML 
		PRINTFORMW 几乎没有对话……
	;会話不能
	ELSEIF TCVAR:70:会話不能
		IF TALENT:70:恋慕 || TALENT:70:思慕
			PRINTFORML 「我刚才说了那件事哦~」
			PRINTFORMW 「因为%CALLNAME:MASTER%你总是不读书，所以话题用尽了！」
			PRINTFORMW 「我这里刚好有相关的书，『肢体重组册』！ 诶，不是这本吗？」
		ELSE
			PRINTFORMW 「啊哈哈……那件事刚才也做了呢……」
		ENDIF
	;ゲスト角色と同室だと各角色1日1回ずつゲスト会話が発生（茶とは別）
	;4=小鈴が恋慕(同行中だと最優先),3=ゲストと同行中in鈴奈庵,2=ゲストと约会中orゲストが恋慕で同行中,1=それ以外
	ELSEIF GUESTS != ""
		SELECTCASE GUESTS
		CASE "霊夢"
			IF GUEST == 4
				IF INRANGE(CFLAG:70:現在位置, 0, 100)
					PRINTFORML 「灵梦，这里的神社有什么用处吗？」
					PRINTFORMW 「什，什么？还有恋爱的作用吗？」
				ELSE
					PRINTFORML 「怎样才能变得像灵梦那样强呢？」
					PRINTFORMW 「首先不要在平时抱来抱去……嗯？」
				ENDIF
			ELSEIF GUEST == 3
				IF !RAND:2
					PRINTFORML 「有事吗？」
					PRINTFORMW 「因为是对灵梦的关照，所以请慢慢地去做」
				ELSE
					PRINTFORMW 「对了！正好在这本书中，可以占卜两个人的缘分哦！」
					PRINTFORML 「那么，是灵梦自己做的呢……」
					PRINTFORMW 「即使如此，我们也可以做！坚持！」
				ENDIF
			ELSEIF GUEST == 2
				PRINTFORML 「和灵梦最近很要好呢」
				PRINTFORMW 「看，现在还牵着你的手」
				PRINTFORML 「什么什么？你没注意到吗？」
				PRINTFORMW 「呵呵，脸变红了哦」
			ELSE
				PRINTFORML 「对了，正好有灵梦，有想说的事情」
				PRINTFORMW 「最近在村子里有传闻――」
			ENDIF
		CASE "魔理沙"
			IF GUEST == 4
				IF INRANGE(CFLAG:70:現在位置, 500, 600) && FLAG:住宿人物 != 70
					IF MAIN_MAP == 5
						PRINTFORMW 「%CALLNAME:MASTER%先生和魔理沙小姐都住在这样的森林里呢」
					ELSE
						PRINTFORMW 「魔理沙小姐住在这样的森林里吧」
					ENDIF
					PRINTFORML 「好啊，我也想在这里生活……」
					PRINTFORMW 「但就算这么说，这里浓密的妖气还是令人不想接近～！」
				ELSE
					PRINTFORML 「魔理沙先生的扫帚很好啊～」
					PRINTFORML 「如果有那个的话，我也能在天空中飞翔」
					PRINTFORMW （和 %CALLNAME:MASTER%先生一起旅行，真浪漫――）
					PRINTL 
					PRINTFORMW 「诶诶！魔法使本来就会飞吗？什么嘛……」
				ENDIF
			ELSEIF GUEST == 3
				IF !RAND:2
					PRINTFORML 「有事吗？」
					PRINTFORMW 「这是我对魔理沙小姐的关照，请慢慢走」
				ELSE
					PRINTFORML 「对了！入手了外面世界的游戏纸牌魔法牌」
					PRINTFORMW 「因为不太清楚魔法的事，所以要不要和魔理沙三个人一起玩玩看呢？」
				ENDIF
			ELSEIF GUEST == 2
				PRINTFORML 「和魔理沙小姐最近很要好呢」
				PRINTFORMW 「看，现在还牵着你的手」
				PRINTFORML 「什么什么？你没注意到吗？」
				PRINTFORMW 「呵呵，脸变红了哦」
			ELSEIF GUEST == 1
				PRINTFORML 「对了，正好有魔理沙小姐，有想跟你说的事情」
				PRINTFORMW 「最近在村子里有传闻――」
			ENDIF
		CASE "紫"
			IF GUEST == 4
				PRINTFORML 「紫是什么人呢……明显感觉是不同的呢……」
				PRINTFORMW 「尽管如此，好像也没有什么特别隐瞒的样子，因为总是神出鬼没的，不知什么时候就消失了……」
				PRINTFORMW 「但是，我觉得，她不一定是很坏的妖怪。」
			ELSEIF GUEST == 3
				PRINTFORML 「嗯，今天你们俩是什么事啊……?」
				PRINTFORMW 「难到又去了什么不好的……?」
			ELSEIF GUEST == 2
				PRINTFORML 「%CALLNAME:MASTER%你知道紫多少个人的事情啊？」
				PRINTFORMW 「似乎你和她亲密了，比我更了解……」
			ELSEIF GUEST == 1
				PRINTFORML 「%CALLNAME:MASTER%你知道神隐的事吗？」
				PRINTFORMW 「是会意外的在身边发生的事情，请小心哦」
			ENDIF
		CASE "文"
			IF GUEST == 4
				IF INRANGE(CFLAG:70:現在位置, 700, 900)
					PRINTFORML 「妖怪之山确实是个可怕的地方」
					PRINTFORMW 「有像文小姐那样的天狗，总觉得很安心呢」
				ELSE
					PRINTFORMW 「也许您知道，文小姐是写『文文新闻』的作者」
					PRINTFORML 「虽然是天狗却经常来人里认真采访」
					PRINTFORMW 「是个不一样的妖怪呢」
				ENDIF
			ELSEIF GUEST == 3
				;筹码は景気の指針
				IF CHARISMA_NOW() >= 100
					PRINTFORML 「文先生的报纸，销路很好哦」
					PRINTFORMW 「在今后的展望上，请继续加印吧」
				ELSE
					PRINTFORML 「文先生的报纸，在这里的销路不好……」
					PRINTFORMW 「要加入什么新的流程比较好」
				ENDIF
				SIF !FLAG:新聞購読
					PRINTFORMW 「啊！%CALLNAME:MASTER%先生也买一部分怎么样？」
			ELSEIF GUEST == 2
				PRINTFORML 「今天你两个人约会吗？」
				PRINTFORMW 「呵呵，如果可以的话，我给你拍照哦」
			ELSEIF GUEST == 1
				PRINTFORML 「咦，难道是文小姐的采访吗？」
				IF TALENT:70:思慕
					PRINTFORML 「文小姐！ %CALLNAME:MASTER%先生的事情的话请务必问我！」
					PRINTFORMW 「\@ CFLAG:70:1005 >= 8 ? 这是我一直引以为豪的， # \@私%CALLNAME:MASTER%先生的事我最了解了！」
				ELSE
					PRINTFORML 「%CALLNAME:MASTER%先生啊」
					PRINTFORMW 「确实%CALLNAME:MASTER%先生是一个很奇怪的人呢」
				ENDIF
			ENDIF
		CASE "猯藏"
			IF GUEST == 4
				PRINTFORML 「对了，%CALLNAME:MASTER%先生对狸猫妖怪的事怎么看？」
				PRINTFORMW 「容易亲近，其实是好妖怪。――」
				SIF ABL:69:親密 >= 5
					PRINTFORMW 「哎呀呀，最近很和睦的……啊!不是!什么都没有!」
			ELSEIF GUEST == 3
				IF TALENT:69:恋慕
					PRINTFORMW 「不过两位还是一样的和睦。」
					PRINTFORMW （但是，真的很有怀疑他们的感情到底是什么?）
				ELSE
					PRINTFORMW 「今天两个人怎么了?？」
					PRINTFORMW （两人都很開心,猯藏小姐的事也不用担心到那里去吧?）
				ENDIF
			ELSEIF GUEST == 2
				PRINTFORML 「两个人，今天的关系都很好呢。」
				PRINTFORMW 「就像是约会一样。」
			ELSEIF GUEST == 1
				PRINTFORML 「%CALLNAME:MASTER%先生您知道狐狸妖怪的事吗？」
				PRINTFORMW 「她会幻化成人的样子在我们身边干坏事的」
			ENDIF
		CASE "阿求"
			;8=独占,7=悲恋,6=揶揄,5=信頼,4=敵対,3=恋慕,2=応援,1=平穏
			SELECTCASE M_KOJO_K70_BALANCE(70, 80)
			CASE 8, 5
				IF FLAG:约会的对象 == 80
					PRINTFORML 「嗯嗯，我并没有想要打扰你。」
					PRINTFORMW 「下次再和我约会吧。♪」
				ELSE
					SELECTCASE RAND:3
					CASE 1
						PRINTFORML 「%CALLNAME:MASTER%先生，让我来告诉您一些情报吧」
						PRINTFORMW 「阿求是一个叫做“幺乐团”的鲜为人知的乐队的大粉丝哦～」
					CASE 2
						PRINTFORMW 「阿求的话，平时很拘束，但是一谈起自己的兴趣就会立刻開朗起来」
						PRINTFORML 「会立刻夸夸其谈，把自己的兴趣完全安利出去」
						PRINTFORMW 「在这一点上她只是一个可爱的丫头呢」
					CASEELSE
						PRINTFORML 「%CALLNAME:MASTER%先生您问阿求到底是个怎样的女孩子？」
						PRINTFORMW 「我老在背后说别人坏话会遭报应的啊！」
					ENDSELECT
				ENDIF
			CASE 7
				SELECTCASE RAND:2
				CASE 1
					PRINTFORML 「%CALLNAME:MASTER%先生，我总是和阿求在一起」
					PRINTFORMW 「偶尔也想和我一起玩啊，什么的」
				CASEELSE
					PRINTFORML 「阿求总是和我偷偷说她和%CALLNAME:MASTER%的恋爱故事」
					PRINTFORMW 「我不想讨厌阿求……所以……」
				ENDSELECT
			CASE 6
				SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「%CALLNAME:MASTER%先生，让我来告诉您一些情报吧」
					PRINTFORMW 「阿求是鲜为人知的幺乐团——传说的乐队的粉丝哦!」
					PRINTFORMW 「如果有机会的话，你一定会高兴的。～」
				CASE 2
					PRINTFORML 「%CALLNAME:MASTER%你到底喜欢阿求什么地方呢?」
					PRINTFORMW 「她有什么我不知道的一面吗? ?」
				CASEELSE
					PRINTFORML 「阿求总是和我偷偷说她和%CALLNAME:MASTER%的恋爱故事」
					PRINTFORMW 「虽然是个很拘谨的家伙，但请好好珍惜吧」
				ENDSELECT
			CASE 4
				IF FLAG:约会的对象 == 80
					PRINTFORML 「看起来%CALLNAME:MASTER%你最近和阿求关系很好呢。」
					PRINTFORMW 「有什么事吗?不是很奇怪吗?」
				ELSEIF M_KOJO_K70_SUZUNAAN(CFLAG:70:現在位置) == 1
					PRINTFORML 「对了，阿求，你的事情已经结束了，我该回去了吗?」
					PRINTFORMW 「客人也来了，坐下来也不好办」
				ELSE
					PRINTFORML 「那个，%CALLNAME:MASTER%，你不去别的地方吗?」
					PRINTFORMW 「啊，不，没有别的意思啦!只是，你在这里，总觉得有点不安……」
				ENDIF
			CASE 3, 1
				PRINTFORML 「阿求是我的朋友。」
				PRINTFORMW 「也许是因为年龄越来越近，所以我已经交往了。」
			CASE 2
				SELECTCASE RAND:2
				CASE 1
					PRINTFORML 「%CALLNAME:MASTER%先生对阿求是怎么想的呢?」
					PRINTFORMW 「这个姑娘很拘谨，很顽固，但是根本的部分还是个女孩子啊~」
				CASEELSE
					PRINTFORML 「阿求总是和我偷偷说她和%CALLNAME:MASTER%的恋爱故事」
					PRINTFORMW 「虽然是个很拘谨的家伙，但请好好珍惜吧」
				ENDSELECT
			ENDSELECT
		ENDSELECT
		;一日一回
		TCVAR:70:(M_KOJO_K70_GUEST_TCVAR(GUEST:1, 0)) = 1
		;约会同行会話と被る場合があるのでこちらを優先させ発生させないようにする
		SETBIT TCVAR:70:360, 0
	;心情不快
	ELSEIF TALENT:70:心情 == -1 || CFLAG:70:ブチギレ || MARK:70:反発刻印 >= 3
		IF TALENT:70:恋慕
			PRINTFORMW 「嗯，对不起，现在有点事……」
		ELSE
			PRINTFORMW 「啊……是这样啊……」
		ENDIF
	;约会会話
	ELSEIF TALENT:70:恋慕 && FLAG:约会的对象 == 70 && DATENAME_SPOT() != "道中" && !约会会話:(CFLAG:70:現在位置 / 10)
		POINT = CFLAG:70:現在位置 / 10
		;こんなにこだわるつもりはなかったのに、某美人妻がたくさん喋るのに釣られてつい
		;新たに地点ができたら修正します（多分）
		SELECTCASE CFLAG:70:约会中
		CASE 0
			IF POINT == 5
				PRINTFORML 「那个，这里是妖精的家吗?」
				PRINTFORMW 「通常我看它只是一棵普通的树......真是不可思议......」
			ELSEIF POINT == 6
				PRINTFORML 「哇，神社旁边竟然有这样的遗迹」
				PRINTFORMW 「真是不可思议的造型啊……真的是遗迹吗?」
			ELSE
				PRINTFORML 「还是这里踏实啊。」
				PRINTFORMW 「看妖怪们来玩的样子，我总觉得有些奇怪。」
			ENDIF
		CASE 1
			IF POINT == 15
				PRINTFORML 「啊~，为什么来墓地……」
				PRINTFORMW 「快去外面吧~ !」
			ELSEIF POINT == 16
				PRINTFORML 「我知道，这个道场是仙界的一部分。」
				PRINTFORMW 「是仙人呀~，%CALLNAME:MASTER%想要当仙人吗？」
			ELSE
				PRINTFORML 「这里有很多妖怪也住在这里啊……」
				PRINTFORMW 「嗯，明明是妖怪，却又装作在做好事吗?」
			ENDIF
		CASE 2
			IF POINT == 21 || POINT == 22
				PRINTFORML 「%CALLNAME:MASTER%先生%CALLNAME:MASTER%先生！ 今天就去那里吧！」
				PRINTFORMW 「嘿嘿，下次约会的时候我决定去哪了♪」
			ELSEIF POINT == 23
				PRINTFORML 「%CALLNAME:MASTER%先生，从这个时间開始就是喝酒时间了吗?」
				PRINTFORMW 「喝太多的话就不能回去了」
			ELSEIF POINT == 24
				PRINTFORML 「我以前也去这里了哦」
				PRINTFORMW 「啊，好可爱啊！这张涂鸦还存在呢」
			ELSEIF POINT == 25
				IF DAY * 1440 + TIME - EX:MASTER:上次的入浴時间 < 360
					PRINTFORML 「嗯~！真舒服！」
					PRINTFORMW 「呜嗯呜嗯♪」
				ELSE
					PRINTFORML 「这里的澡堂我也偶尔来」
					PRINTFORMW 「还是宽敞的浴室很舒服呢。～！」
				ENDIF
			ELSEIF POINT == 26
				;入った時点で合意取得するのね…,诶嘿嘿前か後かは絶頂したかで判断するので事前に絶頂させてると稍微齟齬
				IF EX:70:Ｃ絶頂 + EX:70:Ｖ絶頂 + EX:70:Ａ絶頂 + EX:70:Ｂ絶頂 + EX:70:Ｍ絶頂
					IF TALENT:70:処女
						PRINTFORMW 「……\/\/\/」
					ELSE
						PRINTFORMW 「……%UNICODE(0x2764)%」
					ENDIF
				ELSE
					IF TALENT:70:処女
						PRINTFORMW 「那个……还是温柔的拜托你……\/\/\/」
					ELSE
						PRINTFORMW 「哦……%UNICODE(0x2764)%」
					ENDIF
				ENDIF
			ELSE
				PRINTFORML 「这里主要是居住区」
				PRINTFORMW 「你也去阿求的家玩吗?」
				;と言いつつ行こうとすると拒否されちゃいます。女の子って難しいね
			ENDIF
		CASE 3
			IF POINT == 31
				IF DAY:2 == 2
					PRINTFORML 「嗯，我有点想游泳了。」
					PRINTFORMW 「但是这里的湖泊很危险，不能进去。」
				ELSE
					PRINTFORML 「这里的湖有安全的地方吗？」
					PRINTFORMW 「你会听到大海马啦、人鱼啦、白色的大蛇啦、各种各样的目击故事」
				ENDIF
			ELSEIF POINT == 32
				PRINTFORML 「为什么我在这种地方呆着啊……」
				IF DAY:2 == 2
					PRINTFORMW 「很凉快啊……」
				ELSE
					PRINTFORMW 「嗯……太冷了……」
				ENDIF
			ELSEIF POINT == 33
