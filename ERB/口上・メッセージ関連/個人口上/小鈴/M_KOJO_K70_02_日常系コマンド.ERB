	IF TFLAG:SELECTCOM的分岐
		;初めて
		IF FIRSTTIME("告白")
			IF TALENT:70:恋慕
				PRINTFORMW 「嗯……高兴得不得了～～っ！」
				PRINTL 
				PRINTFORMDL 　紧紧相拥
				PRINTFORMDL 小玲喜极而泣，把%CALLNAME:MASTER%紧紧地抱住
				PRINTW 
				PRINTFORMW 「我也喜欢%CALLNAME:MASTER%先生的啊……！」
				PRINTFORMW 「但是，我很害怕说出口……我总是害怕，说出来关系就会崩溃的吧……」
				PRINTFORMW 「所以我很高兴……现在，我非常幸福……%UNICODE(0x2764)%」
				;日記を書く
				CALL M_KOJO_DIARYSETTING_K70(43)
			ELSE
				PRINTFORMW 「诶……诶诶ーーっ！？」
				PRINTFORMW 「真、真的吗……？」
				PRINTFORMW 「那么…突然、開口……不过……」
				PRINTFORMW 「不过……、可、可以的呢……如果是%CALLNAME:MASTER%先生的话……」
				PRINTFORMW 「虽然会有点不安、但如果是%CALLNAME:MASTER%先生的话一定会好好疼爱我的……」
				PRINTFORMW 「我也喜欢%CALLNAME:MASTER%先生…所以……」
				;日記を書く
				CALL M_KOJO_DIARYSETTING_K70(44)
			ENDIF
		;一度フった,都合のいい奴じゃ！
		ELSE
			;告白口上と同じ,こちらは恋慕のみしかない、はず
			PRINTFORMW 「真，真的可以吗……？ 真，真的可以吗……？」
			PRINTFORMW 「嗯，我很高兴……太好了……」
			PRINTFORMW 「我是不是被讨厌了呢……一直都很害怕……」
			PRINTFORMW 「但是，现在看没有放弃希望，真是太好了……%CALLNAME:MASTER%先生能够接受我……真的是太好了……」
			;日記を書く
			CALL M_KOJO_DIARYSETTING_K70(45)
		ENDIF
		IF TALENT:70:恋慕
			PRINTL 
			PRINTFORMDL 恋が成就した二人は静かに寄り添い、甜空気に包まれてどちらかともなく接吻を交わした……
			PRINTW 
			EXP:70:接吻経験 += 1
			EXP:70:Ｍ経験 += 1
			EXP:MASTER:Ｍ経験 += 1
			EXP:MASTER:接吻経験 += 1
			IF FIRSTTIME("初吻")
				CALL SET_FIRSTKISS(70)
				;［告白］の条件が二人きり
				IF M_KOJO_K70_SUZUNAAN(CFLAG:70:現在位置) == 1
					CSTR:70:初吻喪失履歴 = %PRINT_DATE_F(DAY)%、誰も居なくなった静かな鈴奈庵で恋い焦がれていた%CALLNAME:MASTER%の告白を受け、幸せな情緒の中で契りの初吻を捧げた
				ELSEIF M_KOJO_K70_SUZUNAAN(CFLAG:70:現在位置) == 2
					CSTR:70:初吻喪失履歴 = %PRINT_DATE_F(DAY)%、自分的房間で恋い焦がれていた%CALLNAME:MASTER%の告白を受け、幸せな情緒の中で契りの初吻を捧げた
				ELSEIF FLAG:约会的对象 == 70
					CSTR:70:初吻喪失履歴 = %PRINT_DATE_F(DAY)%、%DATENAME_PLACE()%で约会中、%DATENAME_PLACE()%で恋い焦がれていた%CALLNAME:MASTER%の告白を受け、幸せな情緒の中で契りの初吻を捧げた
				ELSE
					CSTR:70:初吻喪失履歴 = %PRINT_DATE_F(DAY)%、%NAME_FROM_PLACE(CFLAG:70:現在位置)%で恋い焦がれていた%CALLNAME:MASTER%の告白を受け、幸せな情緒の中で契りの初吻を捧げた
				ENDIF
			ENDIF
			CALL M_KOJO_COLOR_K70
			PRINTFORMW 「呼呼……%UNICODE(0x2764)% 从今往后、谁也不知道会发生什么呢――」
			PRINTFORMW 「但一直在一起吧♪ %CALLNAME:MASTER%先生%UNICODE(0x2764)%」
		ENDIF
	;言葉に詰まる
	ELSE
