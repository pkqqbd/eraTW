;==================================================
;相手から约会のお誘い
;==================================================
@M_KOJO_EVENT_K70_27(ARG,ARG:1)
;口上主はLOCAL
;ARG:1は目的地
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
	CASE 0
		;-------------------------------------------------
		;誘い文句
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE STR:(5000 + ARG:1)
			CASE "博麗神社"
				PRINTFORML 「那个，如果可以的话一起去博丽神社吧？」
				PRINTFORMW 「前几天我很在意灵梦小姐借的书是怎样的♪」
			CASE "命蓮寺"
				PRINTFORML 「那个，如果可以的话一起去命莲寺吧？」
				PRINTFORMW 「呵呵，我，即使不修行也能读过♪」
			CASE "人間之里"
				PRINTFORML 「那个，如果可以的话一起去人间之里吧？」
				PRINTFORMW 「那个……想约会……\/\/\/」
			CASE "霧之湖～紅魔館"
				PRINTFORML 「那个，如果可以的话一起去雾之湖吧？」
				PRINTFORMW 「偶尔悠闲地散步吧♪」
			CASE "迷途竹林"
				PRINTFORML 「那个，如果可以的话一起去迷途竹林吧？」
				PRINTFORMW 「竹林的客人，总是迷路，怎么也见不到呢～」
			CASE "魔法之森"
				PRINTFORML 「那个，如果可以的话一起去魔法之森吧？」
				PRINTFORMW 「我很在意前几天魔理沙小姐借的书是怎样的♪」
			CASE "三途之川～冥界"
				PRINTFORML 「那个，如果可以的话一起去中有之道吧？」
				PRINTFORMW 「你不是去死的！在找客人在找客人！」
			CASE "妖怪之山 (山麓)"
				PRINTFORML 「那个，如果可以的话一起去山麓吧？」
				PRINTFORMW 「我借了河童们喜欢的书」
			CASE "妖怪之山 (山頂)"
				PRINTFORML 「那个，如果可以的话一起去山上的神社吧？」
				PRINTFORMW 「那边的神社经常能借书，去回收是很难的」
			CASE "地底"
				PRINTFORML 「那个，我想去地底世界看看！」
				PRINTFORMW 「有点害怕，不过%CALLNAME:MASTER%先生一起来就不怕了……」
			CASE "夢之世界～月"
				PRINTFORML 「那个，我想去月球旅行！」
				PRINTFORMW 「真，真的很浪漫啊……！」
			ENDSELECT
			RETURN 1
		ENDIF
	CASE 1
		;-------------------------------------------------
		;受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORMW 「好啊！没关系，准备好了♪」
			TCVAR:70:354 = 1
			TALENT:70:心情 = 1
			RETURN 1
		ENDIF
	CASE 2
		;-------------------------------------------------
		;断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「是啊，不要介意」
			PRINTFORMW 「那么下次再找机会吧」
			RETURN 1
		ENDIF
	CASE 3
		;-------------------------------------------------
		;部屋に誘われた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「那个，今天不来我家吗……？」
			IF FIRSTTIME("招待", 1, 70)
				;どんな事情だ,看店しなくていいんか
				PRINTFORMW 「因为今天父母都外出了……那个……」
			ELSE
				SELECTCASE RAND:3
				CASE 1
					PRINTFORMW 「%CALLNAME:MASTER%先生有想要读的书……」
				CASE 2
					PRINTFORMW 「我有件事想请你告诉我……」
				CASEELSE
					PRINTFORMW 「今天父母都出去了……」
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
	CASE 4
		;-------------------------------------------------
		;部屋に誘われたのを受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF FIRSTTIME("招待", 0, 70)
				PRINTFORML 「真的吗！？全弄完了！」
				PRINTFORMW （房间弄得很漂亮，真是太好了……）
			ELSE
				PRINTFORML 「哇♪」
				PRINTFORMW 「那就赶紧去吧！」
			ENDIF
			TALENT:70:心情 = 1
			TCVAR:70:354 = 1
			RETURN 1
		ENDIF
	CASE 5
		;-------------------------------------------------
		;部屋に誘われたのを断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「是啊……啊，别介意」
			PRINTFORMW 「下次的机会一定……！」
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

