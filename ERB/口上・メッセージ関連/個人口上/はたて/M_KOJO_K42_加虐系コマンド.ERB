;==================================================
;100,打屁股
;==================================================
@M_KOJO_MESSAGE_COM_KX_100
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_KX_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_KX_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML ぱぁんっ！
		IF TALENT:恋慕 || TALENT:恋人
			PRINTFORML 「いたぁい！　%MASTERNAME:42%、何するのよォ～～」
			PRINTW 
			PRINTFORML いい尻をしていたので、つい…
			PRINTFORML 「女の子のお尻を叩くなんて、%MASTERNAME:42%、ヒドーい！」
			PRINTW 
		ELSE
			PRINTFORML 「いったぁ！　何するのよぉ！」
			PRINTW 
			PRINTFORML いい尻をしていたので、つい…
			PRINTFORML 「つい、じゃないわよ！　ふざけんなっての！」
			PRINTW 
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:愛欲 || TALENT:炮友
		IF TALENT:TARGET:受虐狂 == 1 || ABL:TARGET:受虐属性 >= 2
			IF TEQUIP:Ｖ性交
					PRINTFORML 「あっ%UNICODE(0x2665) *1%　はぁっ%UNICODE(0x2665) *1%　いいっ%UNICODE(0x2665) *1%　もっとっ%UNICODE(0x2665) *1%」
					PRINTFORML 極が性交に酔いしれているさ中、無意識に彼女の尻を揉みしだいていた
					PRINTW 
					PRINTFORML 何とも心地よい感触だ。いい尻をしている…
					PRINTFORML ほとんど無意識で、極の尻を平手で叩く。ぱぁんと小気味良い音が響いた
					PRINTW 
					PRINTFORML 「きゃうんっ%UNICODE(0x2665) *1%」
					PRINTFORML 「あ…ち、違っ、今のは、入れられるのが気持ちよくって…」
					PRINTW 
					PRINTFORML 極はそう言い訳したものの、明らかに抽挿とは違うタイミングでの喘ぎ声だった
					PRINTFORML どうやら極は、叩かれて喜ぶ変態らしい
					PRINTW 
			ELSEIF TEQUIP:Ａ性交
			ELSE
			ENDIF
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML ぺちんっ
				PRINTFORML 「アイタッ。稍微ぉ、女の子のお尻を軽々と叩かないでよぉ」
				PRINTW 
			CASE 1
				PRINTFORML ぱぁんっ！
				PRINTFORML 「いったぁ！　何するのよぉ！」
				PRINTW 
				PRINTFORML すぱぁんっ！
				PRINTFORML 「痛いっ！　だからっ、やめっ…」
				PRINTW 
				PRINTFORML ぱんぱぁんっ！
				PRINTFORML 「ぎゃっ！？　いっ！？　アンタ、いー加減にしないと…」
				PRINTW 
				PRINTFORML ぱぱぱぁん！
				PRINTFORML 「いー加減にしろこのボケェ！！！！」
				PRINTW 
				PRINTFORML ばしぃぃぃぃぃん！
				PRINTFORML 極に思いっきりCounterをくらった
			ENDSELECT
		ENDIF
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

