;==================================================
;100,打屁股
;==================================================
@M_KOJO_MESSAGE_COM_KX_100
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_KX_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_KX_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 啪！
		IF TALENT:恋慕 || TALENT:恋人
			PRINTFORML 「好痛！　%MASTERNAME:42%、你在做什么呀～～」
			PRINTW 
			PRINTFORML 因为这么漂亮的屁股就在眼前、所以就…
			PRINTFORML 「拍打女孩子的屁股什么的、%MASTERNAME:42%、太过分了！」
			PRINTW 
		ELSE
			PRINTFORML 「好痛！　你在干什么呀！」
			PRINTW 
			PRINTFORML 因为这么漂亮的屁股就在眼前、所以就…
			PRINTFORML 「才不是所以就、什么的吧！　開什么玩笑啊！」
			PRINTW 
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:愛欲 || TALENT:炮友
		IF TALENT:TARGET:受虐狂 == 1 || ABL:TARGET:受虐属性 >= 2
			IF TEQUIP:Ｖ性交
					PRINTFORML 「啊%UNICODE(0x2665) *1%　哈啊%UNICODE(0x2665) *1%　好棒%UNICODE(0x2665) *1%　更多%UNICODE(0x2665) *1%」
					PRINTFORML 极沉浸于性爱中的时候、无意识地就開始揉着她的屁股
					PRINTW 
					PRINTFORML 这是何等舒适的手感啊。真是美妙的屁股…
					PRINTFORML 几乎是无意识的、用手掌拍打着极的屁股。啪的一声响起了悦耳的声音
					PRINTW 
					PRINTFORML 「Kya昂%UNICODE(0x2665) *1%」
					PRINTFORML 「啊…等、不是、刚才的是、因为插进去很舒服才…」
					PRINTW 
					PRINTFORML 极虽然如此辩解着、但那明显是和抽插时候不同的娇喘声
					PRINTFORML 看起来极、是个被拍打就会感觉到喜悦的变态呢
					PRINTW 
			ELSEIF TEQUIP:Ａ性交
			ELSE
			ENDIF
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 啪唧！
				PRINTFORML 「啊好痛。等下、不要随便就拍打女孩子的屁股呀」
				PRINTW 
			CASE 1
				PRINTFORML 啪！
				PRINTFORML 「好痛！　你在干什么啊！」
				PRINTW 
				PRINTFORML 啪！
				PRINTFORML 「好痛！　所以说、快停…」
				PRINTW 
				PRINTFORML 啪啪！
				PRINTFORML 「Gya！？　咿！？　我说你、快点ー住手…」
				PRINTW 
				PRINTFORML 啪啪啪！
				PRINTFORML 「快点给我住手ー你这傻子！！！！」
				PRINTW 
				PRINTFORML 砰nnnnnnn！
				PRINTFORML 被极全力地反击了
			ENDSELECT
		ENDIF
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

