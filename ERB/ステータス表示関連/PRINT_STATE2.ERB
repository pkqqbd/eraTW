@PRINT_STATE_GIFTDATA(選択中角色ID, 最大表示行数)
#DIM 選択中角色ID
#DIM 最大表示行数
#DIM 表示開始
SIF !GIFT_GOT:選択中角色ID:1
	RETURN

SIF !最大表示行数
	最大表示行数 = 100

FOR LOCAL, 0, 100
	;フラグ0番が100を兼ねているのでこの形に
	IF !GIFT_GOT:選択中角色ID:((LOCAL + 1) % 100)
		表示開始 = MAX(LOCAL + 1 - 最大表示行数, 1)
		BREAK
	ENDIF
NEXT

