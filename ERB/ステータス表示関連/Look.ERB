			ENDIF
		ENDIF
		CALL LOOK_GIFT(TARGET)
	ENDIF
	IF FLAG:画像表示
		SIF !TFLAG:LOOK非表示
			PRINTL 　
		CALL PRINT_TARGET_IMAGE(TARGET)
	ENDIF
	IF !TFLAG:LOOK非表示
